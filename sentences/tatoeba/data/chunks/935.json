[
  {
    "id": "3488407",
    "transcription": "[12|じゅう|に][歳|さい]です。",
    "translations": {
      "eng": "I'm twelve.",
      "pol": "Ma 12 lat.",
      "fin": "Olen kaksitoistavuotias."
    },
    "text": "12歳です。"
  },
  {
    "id": "3488410",
    "transcription": "トムのことなど[気|き]にするな。",
    "translations": {
      "eng": "Don't pay attention to Tom.",
      "epo": "Ignoru Tomon."
    },
    "text": "トムのことなど気にするな。"
  },
  {
    "id": "3488464",
    "transcription": "トム[怒|おこ]ってるかな？",
    "translations": {
      "eng": "Is Tom mad?",
      "swe": "Undrar om Tom är arg?",
      "deu": "Ob Tom wohl böse ist?",
      "fin": "Onkoha Tom vihane?",
      "cmn": "汤姆是不是生气了？",
      "nld": "Is Tom boos?"
    },
    "audio": {
      "id": "215398",
      "author": "huizi99"
    },
    "text": "トム怒ってるかな？"
  },
  {
    "id": "3488498",
    "transcription": "[変|へん]なの！",
    "translations": {
      "eng": "That's odd!",
      "spa": "¡Es que es extraño!",
      "pol": "Dziwne!",
      "fin": "Outoo!"
    },
    "text": "変なの！"
  },
  {
    "id": "3488505",
    "transcription": "[妙|みょう]だな。",
    "translations": {
      "por": "Que estranho.",
      "eng": "How odd.",
      "rus": "Как странно.",
      "deu": "Das ist ja seltsam.",
      "ind": "Itu aneh.",
      "spa": "Curioso, ¿no?",
      "pol": "To dziwne.",
      "fin": "Onpa outoo.",
      "khm": "ចម្លែកមែន។",
      "tok": "nasa a."
    },
    "audio": {
      "id": "751025",
      "author": "lowteq"
    },
    "text": "妙だな。"
  },
  {
    "id": "3488527",
    "transcription": "[気色|き|しょく][悪|わる]いー！",
    "translations": {
      "eng": "It's weird.",
      "ind": "Menjijikkan!",
      "fin": "Yäk!",
      "pol": "To wstrętne!"
    },
    "text": "気色悪いー！"
  },
  {
    "id": "3488530",
    "transcription": "[風|かぜ]が[強|つよ]いな。",
    "translations": {
      "eng": "It's windy.",
      "fin": "Tuuleepa kovasti."
    },
    "text": "風が強いな。"
  },
  {
    "id": "3488564",
    "transcription": "[寄|よ]りかかっていいよ。",
    "translations": {
      "eng": "You can lean on me.",
      "pol": "Możesz na mnie liczyć."
    },
    "text": "寄りかかっていいよ。"
  },
  {
    "id": "3488643",
    "transcription": "[中|なか]に[入|はい]ってもらって。",
    "translations": {
      "eng": "Let him in.",
      "vie": "Hãy để người đó vào.",
      "tha": "ให้เขาเข้ามา"
    },
    "text": "中に入ってもらって。"
  },
  {
    "id": "3488654",
    "transcription": "[死|し]なせてくれ。",
    "translations": {
      "eng": "Let me die!",
      "por": "Deixe-me morrer.",
      "spa": "¡Déjame morir!",
      "fra": "Laisse-moi mourir.",
      "epo": "Lasu min morti.",
      "fin": "Anna minun kuolla.",
      "vie": "Hãy để cho tôi chết."
    },
    "text": "死なせてくれ。"
  },
  {
    "id": "3488677",
    "transcription": "お[祈|いの]りしましょう。",
    "translations": {
      "eng": "Let's pray.",
      "cmn": "让我们开始祈祷吧。"
    },
    "text": "お祈りしましょう。"
  },
  {
    "id": "3488689",
    "transcription": "これ[見|み]て。",
    "translations": {
      "eng": "Look at it.",
      "pol": "Popatrz na to.",
      "fin": "Kato tätä."
    },
    "text": "これ見て。"
  },
  {
    "id": "3488696",
    "transcription": "お[静|しず]かに。",
    "translations": {
      "eng": "Quiet down.",
      "fra": "Silence.",
      "pol": "Bądź cicho.",
      "slk": "Buďte ticho!"
    },
    "text": "お静かに。"
  },
  {
    "id": "3490642",
    "transcription": "[最近|さい|きん][YouTube|ユーチューブ]がやけに[重|おも]い[気|き]がする。",
    "translations": {
      "eng": "Recently YouTube feels terribly slow."
    },
    "text": "最近YouTubeがやけに重い気がする。"
  },
  {
    "id": "3490643",
    "transcription": "[最近|さい|きん]やけに[人身|じん|しん][事故|じ|こ][多|おお]くない？",
    "translations": {
      "eng": "Haven't there been a lot of awful accidents recently?"
    },
    "text": "最近やけに人身事故多くない？"
  },
  {
    "id": "3491404",
    "transcription": "[夜|よる][遅|おそ]くまで[開|あ]いている[博物館|はく|ぶつ|かん]か[美術館|び|じゅつ|かん]はありませんか？",
    "translations": {
      "fra": "Y a-t-il des musées ouverts tard dans la nuit ?",
      "rus": "А нет ли музеев или картинных галерей, которые открыты до позднего вечера?",
      "eng": "Are there any museums or art galleries open until late evening?"
    },
    "text": "夜遅くまで開いている博物館か美術館はありませんか？"
  },
  {
    "id": "3491511",
    "transcription": "ほんとに[私|わたし]のこと[好|す]きなの？",
    "translations": {
      "fra": "M'aimes-tu vraiment ?",
      "cmn": "你真的愛我嗎？",
      "yue": "你係咪真係鍾意我㗎？",
      "rus": "Я правда тебе нравлюсь?",
      "fin": "Rakastatko minua ihan oikeasti?",
      "deu": "Hast du mich wirklich lieb?",
      "ind": "Kamu benar-benar suka sama aku?"
    },
    "audio": {
      "id": "215503",
      "author": "yomi"
    },
    "text": "ほんとに私のこと好きなの？"
  },
  {
    "id": "3491551",
    "transcription": "トムはため[息|いき]をついた。",
    "translations": {
      "deu": "Tom seufzte.",
      "eng": "Tom gave a sigh.",
      "rus": "Том вздохнул.",
      "ita": "Tom sospirò."
    },
    "audio": {
      "id": "215504",
      "author": "huizi99"
    },
    "text": "トムはため息をついた。"
  },
  {
    "id": "3491565",
    "transcription": "[次|つぎ]は[誰|だれ]？",
    "translations": {
      "spa": "¿Siguiente?",
      "eng": "Who's next?",
      "pol": "Kto następny?",
      "eus": "Nor da hurrena?",
      "hrv": "Tko je sljedeći?",
      "cmn": "下一个是谁？",
      "fin": "Kukas sitte?",
      "ind": "Selanjutnya siapa?",
      "fra": "Qui sera le suivant ?"
    },
    "text": "次は誰？"
  },
  {
    "id": "3491589",
    "transcription": "[言|い]うとおりにして。",
    "translations": {
      "deu": "Tu, was ich sage!",
      "cmn": "按我說的做。",
      "vie": "Hãy làm như tôi nói."
    },
    "text": "言うとおりにして。"
  },
  {
    "id": "3491596",
    "transcription": "[撃|う]たないで！",
    "translations": {
      "eng": "Don't shoot.",
      "deu": "Nicht schießen!",
      "fin": "Älä ammu!",
      "pol": "Nie strzelaj!"
    },
    "text": "撃たないで！"
  },
  {
    "id": "3491598",
    "transcription": "[怒鳴|ど|な]らないで。",
    "translations": {
      "eng": "Don't shout."
    },
    "text": "怒鳴らないで。"
  },
  {
