    "translations": {
      "eng": "I've got a son.",
      "fra": "J'ai un fils.",
      "rus": "У меня есть один сын.",
      "pol": "Mam jednego syna.",
      "ces": "Mám jednoho syna.",
      "ukr": "У мене є один син."
