[
  {
    "id": "8689008",
    "transcription": "トムって[結婚|けっ|こん]してるの？",
    "translations": {
      "eng": "Is Tom married?"
    },
    "text": "トムって結婚してるの？"
  },
  {
    "id": "8689037",
    "transcription": "[手|て]、[空|あ]いてる？",
    "translations": {
      "eng": "Are you available?"
    },
    "text": "手、空いてる？"
  },
  {
    "id": "8689054",
    "transcription": "あなたの[名前|な|まえ]はトムなの？",
    "translations": {
      "dan": "Er dit navn Tom?",
      "epo": "Ĉu via nomo estas Tomo?",
      "deu": "Ist dein Name Tom?"
    },
    "text": "あなたの名前はトムなの？"
  },
  {
    "id": "8689061",
    "transcription": "[飲|の]んでたの？",
    "translations": {
      "eng": "Were you drinking?",
      "deu": "Hast du getrunken?",
      "slk": "Pila si?"
    },
    "text": "飲んでたの？"
  },
  {
    "id": "8689062",
    "transcription": "[君|きみ]、トムなの？",
    "translations": {
      "eng": "Are you Tom?",
      "deu": "Bist du Tom?",
      "epo": "Ĉu vi estas Tomo?",
      "fra": "Es-tu Tom ?",
      "cmn": "你是汤姆吗？",
      "rus": "Ты Том?",
      "lat": "Es tu Tom?",
      "ukr": "Ти Том?",
      "nld": "Ben jij Tom?"
    },
    "text": "君、トムなの？"
  },
  {
    "id": "8689067",
    "transcription": "メモ、[取|と]ってるの？",
    "translations": {
      "eng": "Are you taking notes?",
      "deu": "Machst du dir Notizen?",
      "spa": "¿Estás tomando notas?"
    },
    "text": "メモ、取ってるの？"
  },
  {
    "id": "8689102",
    "transcription": "あの[人|ひと]、[君|きみ]の[妹|いもうと]？",
    "translations": {
      "deu": "Ist das deine Schwester?",
      "eng": "Is that person your younger sister?",
      "vie": "Người đó là em gái của bạn à?"
    },
    "text": "あの人、君の妹？"
  },
  {
    "id": "8689105",
    "transcription": "[今日|きょう]って、[金曜日|きん|よう|び]？",
    "translations": {
      "eng": "Is today Friday?",
      "ita": "Oggi è venerdì?",
      "deu": "Ist heute Freitag?"
    },
    "text": "今日って、金曜日？"
  },
  {
    "id": "8689109",
    "transcription": "[終|お]わった？",
    "translations": {
      "spa": "¿Terminó?"
    },
    "text": "終わった？"
  },
  {
    "id": "8689114",
    "transcription": "トムって、[入院|にゅう|いん][中|ちゅう]なの？",
    "translations": {
      "eng": "Is Tom in the hospital?",
      "rus": "Том в больнице?",
      "deu": "Ist Tom im Krankenhaus?",
      "epo": "Ĉu Tomo estas en la malsanulejo?",
      "nds": "Is Tom in’t Krankenhuus?",
      "vie": "Tom đang nhập viện có đúng không vậy?"
    },
    "text": "トムって、入院中なの？"
  },
  {
    "id": "8689122",
    "transcription": "ボランティアの[方|かた]はいらっしゃいますか？",
    "translations": {
      "eng": "Are there any volunteers?",
      "nob": "Er det noen frivillige?"
    },
    "text": "ボランティアの方はいらっしゃいますか？"
  },
  {
    "id": "8689123",
    "transcription": "それって、[君|きみ]の[自転車|じ|てん|しゃ]？",
    "translations": {
      "eng": "Is that your bicycle?",
      "dan": "Er det din cykel?",
      "deu": "Ist das dein Fahrrad?",
      "nds": "Is dat dien Fohrrad?"
    },
    "text": "それって、君の自転車？"
  },
  {
    "id": "8689124",
    "transcription": "[先生|せん|せい]のこと[好|す]き？",
    "translations": {
      "eng": "Do you like your teachers?",
      "deu": "Magst du deine Lehrerin?"
    },
    "text": "先生のこと好き？"
  },
  {
    "id": "8689127",
    "transcription": "[死亡|し|ぼう][者|しゃ][数|すう]は[増|ふ]え[続|つづ]けています。",
    "translations": {
      "eng": "The number of deaths is increasing.",
      "deu": "Der Zahl der Toten nimmt immer weiter zu."
    },
    "text": "死亡者数は増え続けています。"
  },
  {
    "id": "8689173",
    "transcription": "[誰|だれ]か[殺|ころ]されたの？",
    "translations": {
      "eng": "Was anybody killed?",
      "fin": "Tapettiinko ketään?"
    },
    "text": "誰か殺されたの？"
  },
  {
    "id": "8689191",
    "transcription": "トムは[昼寝|ひる|ね]をした。",
    "translations": {
      "deu": "Tom machte einen Mittagsschlaf.",
      "tha": "ทอมนอนกลางวันแล้ว"
    },
    "text": "トムは昼寝をした。"
  },
  {
    "id": "8689192",
    "transcription": "トムは[仮眠|か|みん]をとった。",
    "translations": {
      "epo": "Tomo dormetis.",
      "eng": "Tom took a nap.",
      "deu": "Tom machte ein Nickerchen."
    },
    "text": "トムは仮眠をとった。"
  },
  {
    "id": "8689601",
    "transcription": "[泣|な]いてたの？",
    "translations": {
      "eng": "Have you been crying?",
      "epo": "Ĉu vi ploris?",
      "rus": "Ты плакала?",
      "lat": "Plorabasne?",
      "ukr": "Ти плакала?",
      "deu": "Hast du geweint?",
      "por": "Estava chorando?"
    },
    "text": "泣いてたの？"
  },
  {
    "id": "8689603",
    "transcription": "[忙|いそが]しかった？",
    "translations": {
      "eng": "Were you busy?",
      "deu": "Warst du beschäftigt?",
      "por": "Estava uma correria para você?"
    },
    "text": "忙しかった？"
  },
  {
    "id": "8689612",
    "transcription": "ランチの[準備|じゅん|び]ってできてる？",
    "translations": {
      "epo": "Ĉu la tagmanĝo pretas?",
      "nld": "Is het middageten klaar?",
      "rus": "Обед готов?",
      "eng": "Is lunch ready?",
      "fra": "Le déjeuner est-il prêt ?",
      "ukr": "Обід готовий?",
      "deu": "Ist das Mittagessen fertig?"
    },
    "text": "ランチの準備ってできてる？"
  },
  {
    "id": "8689613",
    "transcription": "トムは[寝|ね]てるの？",
    "translations": {
      "eng": "Is Tom sleeping?",
      "rus": "Том спит?",
      "epo": "Ĉu Tomo dormas?",
      "deu": "Schläft Tom?",
      "nld": "Is Tom aan het slapen?",
      "ukr": "Том спить?",
      "npi": "के टम सुतिरहेको छ?"
    },
    "text": "トムは寝てるの？"
  },
  {
    "id": "8689616",
    "transcription": "トムは[泳|およ]いでるの？",
    "translations": {
      "eng": "Is Tom swimming?",
      "deu": "Schwimmt Tom?",
      "cmn": "汤姆在游泳吗？"
    },
    "text": "トムは泳いでるの？"
  },
  {
    "id": "8689620",
    "transcription": "トムって[泣|な]いてたの？",
    "translations": {
      "eng": "Has Tom been crying?",
      "epo": "Ĉu Tom ploris?",
      "rus": "Том плакал?",
      "lat": "Plorabatne Thomas?",
      "ukr": "Том плакав?",
      "deu": "Hat Tom geweint?",
      "pol": "Czy Tom płakał?",
      "nld": "Heeft Tom gehuild?",
      "fra": "Tom a-t-il pleuré ?",
      "fin": "Itkikö Tomi?",
      "spa": "¿Lloró Tom?"
    },
    "text": "トムって泣いてたの？"
  },
  {
    "id": "8689663",
    "transcription": "あなた、[正気|しょう|き]なの？",
    "translations": {
      "eng": "Are you insane?",
      "deu": "Bist du noch bei Trost?"
    },
    "text": "あなた、正気なの？"
  },
  {
    "id": "8689671",
    "transcription": "[諦|あきら]めてるの？",
    "translations": {
      "eng": "Are you giving up?",
      "rus": "Ты сдаёшься?",
      "deu": "Hast du aufgegeben?",
      "fin": "Luovutatko?",
      "spa": "¿Te has rendido?"
    },
    "text": "諦めてるの？"
  },
  {
    "id": "8689688",
    "transcription": "この[傘|かさ]はトムのかしら？",
    "translations": {
      "eng": "Is this umbrella Tom's?",
      "rus": "Это зонтик Тома?"
    },
    "text": "この傘はトムのかしら？"
  },
  {
    "id": "8689692",
    "transcription": "[私|わたし]たちを[探|さが]してるの？",
    "translations": {
      "eng": "Are you looking for us?",
      "deu": "Suchst du uns?",
      "epo": "Ĉu vi serĉas nin?",
      "rus": "Ты нас ищешь?",
      "fra": "Nous cherches-tu ?",
      "pol": "Szukasz nas?"
    },
    "text": "私たちを探してるの？"
  },
  {
    "id": "8689736",
    "transcription": "あの[人|ひと]たち、[私|わたし]たちを[見|み]てる？",
    "translations": {
      "eng": "Are they looking at us?"
    },
    "text": "あの人たち、私たちを見てる？"
  },
  {
    "id": "8689740",
    "transcription": "うちの[旦那|だん|な]、[料理|りょう|り]がとても[上手|うま]いの。",
    "translations": {
      "eng": "My husband is a very good cook.",
      "rus": "Мой муж очень хорошо готовит.",
      "ukr": "Мій чоловік дуже добре готує.",
      "deu": "Mein Mann kann sehr gut kochen."
    },
    "text": "うちの旦那、料理がとても上手いの。"
  },
  {
    "id": "8689758",
    "transcription": "この[話|はなし]って[本当|ほん|とう]なの？",
    "translations": {
      "eng": "Is this story true?",
      "deu": "Stimmt diese Geschichte?"
    },
    "text": "この話って本当なの？"
  },
  {
    "id": "8689762",
    "transcription": "トムってまだ[泣|な]いてるの？",
    "translations": {
      "eng": "Is Tom still crying?",
      "rus": "Том до сих пор плачет?",
      "ukr": "Том ще плаче?",
      "deu": "Weint Tom noch immer?",
      "epo": "Ĉu Tomo ankoraŭ ploras?",
      "nds": "Huult Tom noch jümmer?",
      "spa": "Y Tom, ¿todavía llorando?"
    },
    "text": "トムってまだ泣いてるの？"
  },
  {
    "id": "8689764",
    "transcription": "あなたたち[二人|ふたり]とも[気|き]は[確|たし]かなの？",
    "translations": {
      "eng": "Are you both crazy?",
      "deu": "Seid ihr beide noch bei Trost?"
    },
    "text": "あなたたち二人とも気は確かなの？"
  },
  {
    "id": "8689825",
    "transcription": "これはつまんない。[何|なん]か[楽|たの]しいことしようよ！",
    "translations": {
      "eng": "This is boring. Let's do something fun!",
      "deu": "Das ist langweilig. Lass uns was Vergnügliches machen!"
    },
    "text": "これはつまんない。何か楽しいことしようよ！"
  },
  {
    "id": "8689980",
    "transcription": "トムって[背|せ]は[高|たか]い？",
    "translations": {
      "eng": "Is Tom tall?",
      "deu": "Ist Tom groß?",
      "tha": "คนชื่อทอมสูงไหม"
    },
    "text": "トムって背は高い？"
  },
  {
    "id": "8691397",
    "transcription": "これは[芸術|げい|じゅつ]なの？",
    "translations": {
      "eng": "Is this art?",
      "deu": "Ist das Kunst?"
    },
    "text": "これは芸術なの？"
  },
  {
    "id": "8691418",
    "transcription": "[隠|かく]れてるの？",
    "translations": {
      "eng": "Are you hiding?"
    },
    "text": "隠れてるの？"
  },
  {
    "id": "8691483",
    "transcription": "[可能|か|のう]ですか？",
    "translations": {
      "eng": "Is that possible?",
      "cmn": "有可能吗？",
      "por": "É possível?"
    },
    "text": "可能ですか？"
  },
  {
    "id": "8691500",
    "transcription": "それで[合|あ]ってる？",
    "translations": {
      "eng": "Is that correct?"
    },
    "text": "それで合ってる？"
  },
  {
    "id": "8691629",
    "transcription": "[今|いま]、お[母|かあ]さんと[一緒|いっ|しょ]に[暮|く]らしています。",
    "translations": {
      "eng": "I'm living with my mom now.",
      "deu": "Ich wohne zur Zeit bei meiner Mama."
    },
    "text": "今、お母さんと一緒に暮らしています。"
  },
  {
    "id": "8691631",
    "transcription": "[娘|むすめ]は[私|わたし]と[一緒|いっ|しょ]に[住|す]んでいます。",
    "translations": {
      "rus": "Дочь живёт со мной.",
      "eng": "My daughter lives with me.",
      "deu": "Meine Tochter wohnt bei mir."
    },
    "text": "娘は私と一緒に住んでいます。"
  },
  {
    "id": "8691889",
    "transcription": "[誰|だれ]か[見|み]てる？",
    "translations": {
      "eng": "Is anyone looking?",
      "spa": "¿Hay alguien mirando?"
    },
    "text": "誰か見てる？"
  },
  {
    "id": "8692016",
    "transcription": "あそこに[行|い]くつもりなの？",
    "translations": {
      "eng": "Is that where you intend to go?",
      "deu": "Da willst du also hin?"
    },
    "text": "あそこに行くつもりなの？"
  },
  {
    "id": "8692021",
    "transcription": "トムって[本当|ほん|とう]にマネージャーなの？",
    "translations": {
      "eng": "Is Tom really the manager?",
      "deu": "Ist Tom wirklich der Geschäftsführer?"
    },
    "text": "トムって本当にマネージャーなの？"
  },
  {
    "id": "8692022",
    "transcription": "トムはまだ[怒|おこ]ってるかな？",
    "translations": {
      "fin": "Onkohan Tom vielä vihainen?"
    },
    "text": "トムはまだ怒ってるかな？"
  },
  {
    "id": "8692023",
    "transcription": "トムってまだ[私|わたし]のこと[怒|おこ]ってるかな？",
    "translations": {
      "eng": "Is Tom still mad at me?",
      "deu": "Ist Tom mir wohl noch böse?"
    },
    "text": "トムってまだ私のこと怒ってるかな？"
  },
  {
    "id": "8692025",
    "transcription": "[街|まち]まで[遠|とお]いですか？",
    "translations": {
      "eng": "Is it far to town?",
      "rus": "До города далеко?",
      "deu": "Ist es weit bis zur Stadt?"
    },
    "text": "街まで遠いですか？"
  },
  {
    "id": "8692034",
    "transcription": "[椅子|い|す]は[足|た]りそうですか？",
    "translations": {
      "eng": "Are there enough chairs?",
      "fra": "Y a-t-il suffisamment de chaises ?",
      "deu": "Gibt es genügend Stühle?",
      "rus": "Стульев достаточно?",
      "epo": "Ĉu estas sufiĉe da seĝoj?",
      "nld": "Zijn er genoeg stoelen?",
      "por": "Acha que há cadeiras suficientes?"
    },
    "text": "椅子は足りそうですか？"
  },
  {
    "id": "8692035",
    "transcription": "まだ[高校生|こう|こう|せい]なの？",
    "translations": {
      "eng": "Are you still in high school?",
      "fin": "Oletko vielä lukiolainen?"
    },
    "text": "まだ高校生なの？"
  },
  {
    "id": "8692037",
    "transcription": "トムは[免疫|めん|えき][力|りょく]が[低下|てい|か]している。",
    "translations": {
      "eng": "Tom has a weakened immune system.",
      "deu": "Tom hat ein geschwächtes Immunsystem.",
      "fin": "Tomilla on alentunut vastustuskyky."
    },
    "text": "トムは免疫力が低下している。"
  },
  {
    "id": "8692039",
    "transcription": "あれが[君|きみ]の[家|いえ]？",
    "translations": {
      "eng": "Is that your house?",
      "deu": "Ist das dein Haus?",
      "mfa": "situ yadi rumorh daemor?"
    },
    "text": "あれが君の家？"
  }
]