[
  {
    "id": "8697959",
    "transcription": "はてなマークは[句読点|く|とう|てん]です。",
    "translations": {
      "epo": "Demandosigno estas interpunkcia signo.",
      "eng": "A question mark is a punctuation mark.",
      "deu": "Ein Fragezeichen ist ein Interpunktionszeichen."
    },
    "text": "はてなマークは句読点です。"
  },
  {
    "id": "8699140",
    "transcription": "[武器|ぶ|き]は[持|も]ったか？",
    "translations": {
      "eng": "Are you armed?",
      "slk": "Si ozbrojený?",
      "deu": "Bist du bewaffnet?"
    },
    "text": "武器は持ったか？"
  },
  {
    "id": "8699148",
    "transcription": "[寒|さむ]いの？",
    "translations": {
      "por": "Tá com frio?",
      "eng": "Are you cold?",
      "slk": "Je ti chladno?",
      "vie": "Bạn lạnh không?",
      "ind": "Kamu kedinginan?"
    },
    "text": "寒いの？"
  },
  {
    "id": "8699245",
    "transcription": "[全員|ぜん|いん]ここにいる？",
    "translations": {
      "eng": "Is everybody here?"
    },
    "text": "全員ここにいる？"
  },
  {
    "id": "8699303",
    "transcription": "お[腹|なか]が[痛|いた]いの？",
    "translations": {
      "eng": "Does your stomach hurt?",
      "hun": "Fáj a hasad?"
    },
    "text": "お腹が痛いの？"
  },
  {
    "id": "8699443",
    "transcription": "いつかこんなことになるって、ずっと[思|おも]ってたよ。",
    "translations": {
      "eng": "I always knew this would happen someday."
    },
    "text": "いつかこんなことになるって、ずっと思ってたよ。"
  },
  {
    "id": "8699448",
    "transcription": "トムとメアリーは[狩|か]りに[行|い]ったよ。",
    "translations": {
      "eng": "Tom and Mary have gone hunting.",
      "deu": "Tom und Maria sind jagen gegangen."
    },
    "text": "トムとメアリーは狩りに行ったよ。"
  },
  {
    "id": "8699645",
    "transcription": "これは[珍|めずら]しいことなの？",
    "translations": {
      "eng": "Is this uncommon?",
      "fin": "Onko tämä epätavallista?",
      "vie": "Đây có phải là một chuyện hiếm gặp không?",
      "por": "Isso é raro?"
    },
    "text": "これは珍しいことなの？"
  },
  {
    "id": "8699721",
    "transcription": "[皆|みな]さんは[教員|きょう|いん]なんですか？",
    "translations": {
      "eng": "Are you teachers?",
      "deu": "Sind Sie alle Lehrer?"
    },
    "text": "皆さんは教員なんですか？"
  },
  {
    "id": "8699785",
    "transcription": "[誰|だれ]かが[聞|き]いてくれてたの？",
    "translations": {
      "eng": "Was anyone listening?"
    },
    "text": "誰かが聞いてくれてたの？"
  },
  {
    "id": "8699804",
    "transcription": "トムは[驚|おどろ]いてた？",
    "translations": {
      "eng": "Was Tom surprised?",
      "deu": "War Tom überrascht?",
      "cmn": "汤姆吃惊吗？"
    },
    "text": "トムは驚いてた？"
  },
  {
    "id": "8699808",
    "transcription": "トムは[看護|かん|ご][師|し]だったんですか？",
    "translations": {
      "eng": "Was Tom a nurse?",
      "rus": "Том был медбратом?"
    },
    "text": "トムは看護師だったんですか？"
  },
  {
    "id": "8699811",
    "transcription": "それって、そんなに[驚|おどろ]くこと？",
    "translations": {
      "eng": "Is that so surprising?"
    },
    "text": "それって、そんなに驚くこと？"
  },
  {
    "id": "8699812",
    "transcription": "[家|いえ]に[帰|かえ]るの？",
    "translations": {
      "eng": "Are you going home?",
      "slk": "Idete domov?",
      "spa": "¿Volvés a tu casa?"
    },
    "text": "家に帰るの？"
  },
  {
    "id": "8699912",
    "transcription": "トムが[一緒|いっ|しょ]だったの？",
    "translations": {
      "eng": "Was Tom with you?",
      "cmn": "汤姆和你在一起吗？"
    },
    "text": "トムが一緒だったの？"
  },
  {
    "id": "8699915",
    "transcription": "[外|そと]は[寒|さむ]い？",
    "translations": {
      "slk": "Je vonku chladno?",
      "fin": "Onko ulkona kylmä?",
      "fra": "Est-ce qu'il fait froid dehors ?",
      "spa": "¿Está frío afuera?"
    },
    "text": "外は寒い？"
  },
  {
    "id": "8699916",
    "transcription": "トムは[昨日|きのう][仕事|し|ごと]してた？",
    "translations": {
      "eng": "Was Tom working yesterday?",
      "deu": "Hat Tom gestern gearbeitet?"
    },
    "text": "トムは昨日仕事してた？"
  },
  {
    "id": "8699991",
    "transcription": "[楽|たの]しかった？",
    "translations": {
      "eng": "Was it fun?",
      "rus": "Весело было?",
      "deu": "Hat es Spaß gemacht?",
      "por": "Achou divertido?",
      "mfa": "seda dor?"
    },
    "text": "楽しかった？"
  },
  {
    "id": "8699995",
    "transcription": "[今日|きょう]は[給料|きゅう|りょう][日|び]なの？",
    "translations": {
      "eng": "Is today payday?",
      "rus": "Сегодня день зарплаты?",
      "deu": "Ist heute Zahltag?",
      "vie": "Hôm nay là ngày phát lương có đúng không?"
    },
    "text": "今日は給料日なの？"
  },
  {
    "id": "8700007",
    "transcription": "[本当|ほん|とう]のことを[言|い]ってる？",
    "translations": {
      "eng": "Are you telling me the truth?",
      "fra": "Me dis-tu la vérité ?",
      "epo": "Ĉu vi diras al mi la veron?",
      "rus": "Ты мне правду говоришь?",
      "deu": "Sagst du mir auch die Wahrheit?"
    },
    "text": "本当のことを言ってる？"
  },
  {
    "id": "8700475",
    "transcription": "[独|ひと]り[言|ごと]を[言|い]ってるの？",
    "translations": {
      "eng": "Are you talking to yourself?",
      "deu": "Führst du Selbstgespräche?",
      "rus": "Ты говоришь сама с собой?",
      "fin": "Puhutko itseksesi?"
    },
    "text": "独り言を言ってるの？"
  },
  {
    "id": "8700478",
    "transcription": "また[独|ひと]り[言|ごと]を[言|い]ってるの？",
    "translations": {
      "eng": "Are you talking to yourself again?",
      "fin": "Taasko puhut itsellesi?"
    },
    "text": "また独り言を言ってるの？"
  },
  {
    "id": "8700481",
    "transcription": "どうして[独|ひと]り[言|ごと]を[言|い]ってるの？",
    "translations": {
      "eng": "Why are you talking to yourself?",
      "deu": "Warum führst du Selbstgespräche?",
      "fra": "Pourquoi parles-tu à toi-même ?",
      "nno": "Kvifor pratar du med deg sjølv?"
    },
    "text": "どうして独り言を言ってるの？"
  },
  {
    "id": "8700601",
    "transcription": "みんな[訛|なま]りがある。",
