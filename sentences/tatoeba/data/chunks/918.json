[
  {
    "id": "3366798",
    "transcription": "すぐそこで[待|ま]ってて。",
    "translations": {
      "eng": "Wait right there."
    },
    "text": "すぐそこで待ってて。"
  },
  {
    "id": "3366800",
    "transcription": "[彼|かれ]らは[夫婦|ふう|ふ]だ。",
    "translations": {
      "fin": "He ovat toistensa puolisot.",
      "deu": "Sie sind ein Ehepaar.",
      "rus": "Они супруги.",
      "spa": "Son marido y mujer."
    },
    "text": "彼らは夫婦だ。"
  },
  {
    "id": "3366801",
    "transcription": "これは[直|なお]せる。",
    "translations": {
      "eng": "This can be fixed."
    },
    "text": "これは直せる。"
  },
  {
    "id": "3366804",
    "transcription": "[結婚|けっ|こん]しよう。",
    "translations": {
      "eng": "Let's get married.",
      "spa": "Casémonos.",
      "rus": "Давай поженимся.",
      "deu": "Heiraten wir!"
    },
    "text": "結婚しよう。"
  },
  {
    "id": "3366807",
    "transcription": "まだ[手遅|て|おく]れではない。",
    "translations": {
      "eng": "It isn't too late.",
      "deu": "Es ist noch nicht zu spät."
    },
    "text": "まだ手遅れではない。"
  },
  {
    "id": "3366810",
    "transcription": "[私|わたし]のせいじゃないもん。",
    "translations": {
      "nld": "Het is niet mijn schuld, hoor.",
      "fin": "No ei ole kuule minun vikani.",
      "deu": "Ist doch nicht meine Schuld!"
    },
    "text": "私のせいじゃないもん。"
  },
  {
    "id": "3366811",
    "transcription": "そろそろ[行|い]かなきゃ。",
    "translations": {
      "eng": "It's almost time to go."
    },
    "text": "そろそろ行かなきゃ。"
  },
  {
    "id": "3366812",
    "transcription": "いい[考|かんが]えだ。",
    "translations": {
      "eng": "It's a good idea.",
      "urd": "یہ اچھا آئیڈیا ہے۔",
      "cmn": "这是个好主意。"
    },
    "text": "いい考えだ。"
  },
  {
    "id": "3366813",
    "transcription": "トムはいつ[戻|もど]ってくるの？",
    "translations": {
      "eng": "When will Tom get back?",
      "fin": "Milloin Tom tulee takaisin?"
    },
    "text": "トムはいつ戻ってくるの？"
  },
  {
    "id": "3366814",
    "transcription": "トムって[猫|ねこ]は[好|す]きかな？",
    "translations": {
      "eng": "Does Tom like cats?",
      "fin": "Pitääköhän Tom kissoista?"
    },
    "text": "トムって猫は好きかな？"
  },
  {
    "id": "3366816",
    "transcription": "トムは[泣|な]いていたのだろうか。",
    "translations": {
      "deu": "Tom hat vielleicht geweint."
    },
    "text": "トムは泣いていたのだろうか。"
  },
  {
    "id": "3366817",
    "transcription": "トムは[反対|はん|たい]なのだろうか。",
    "translations": {
      "eng": "Does Tom disagree?",
      "fin": "Onkohan Tom eri mieltä?"
    },
    "text": "トムは反対なのだろうか。"
  },
  {
    "id": "3366819",
    "transcription": "トムってペット[飼|か]っているのかな？",
    "translations": {
      "eng": "Does Tom have a pet?"
    },
    "text": "トムってペット飼っているのかな？"
  },
  {
    "id": "3366820",
    "transcription": "トムって[何|なに]かペットを[飼|か]っているのかな？",
    "translations": {
      "eng": "Does Tom have any pets?",
      "fin": "Onkohan Tomilla joku lemmikkieläin?",
      "fra": "Tu sais si Tom a un animal de compagnie ?"
    },
    "text": "トムって何かペットを飼っているのかな？"
  },
  {
    "id": "3366822",
    "transcription": "[僕|ぼく]はトムに[怒|おこ]っている。",
    "translations": {
      "eng": "I'm mad at Tom.",
      "fin": "Olen suuttunut Tomille."
    },
    "text": "僕はトムに怒っている。"
  },
  {
    "id": "3366823",
    "transcription": "お[茶|ちゃ]を[持|も]ってきて。",
    "translations": {
      "eng": "Bring tea.",
      "deu": "Bring Tee mit."
    },
    "text": "お茶を持ってきて。"
  },
  {
    "id": "3366825",
    "transcription": "[君|きみ]どうしちゃったの？",
    "translations": {
      "rus": "Что с тобой случилось?",
      "eng": "What has become of you?"
    },
    "text": "君どうしちゃったの？"
  },
  {
    "id": "3366827",
    "transcription": "[俺|おれ]たちは[取引|とり|ひき]をした。",
    "translations": {
      "eng": "We made a bargain.",
      "deu": "Wir haben einen Handel gemacht.",
      "fin": "Me tehtiin kaupat."
    },
    "text": "俺たちは取引をした。"
  },
  {
    "id": "3366828",
    "transcription": "どっちの[脚|あし]が[痛|いた]むの？",
    "translations": {
      "eng": "Which leg hurts?",
      "deu": "Welches Bein schmerzt?"
    },
    "text": "どっちの脚が痛むの？"
  },
  {
    "id": "3366830",
    "transcription": "[誰|だれ]にも[言|い]ってない。",
    "translations": {
      "eng": "I haven't told anyone."
    },
    "text": "誰にも言ってない。"
  },
  {
    "id": "3366833",
    "transcription": "トムは[電話|でん|わ]を[取|と]り、[話|はな]し[始|はじ]めた。",
    "translations": {
      "eng": "Tom picked up the phone and started talking.",
      "deu": "Tom nahm den Hörer ab und fing an zu reden.",
      "rus": "Том взял телефон и начал говорить."
    },
    "text": "トムは電話を取り、話し始めた。"
  },
  {
    "id": "3366847",
    "transcription": "[彼|かれ]らはカップルだ。",
    "translations": {
      "eng": "They're a couple.",
      "deu": "Sie sind ein Paar."
    },
    "text": "彼らはカップルだ。"
  },
  {
    "id": "3366849",
    "transcription": "[彼|かれ]らは[付|つ]き[合|あ]っている。",
    "translations": {
      "fin": "He seurustelevat.",
      "eng": "They’re going out together.",
      "rus": "Они встречаются.",
      "vie": "Họ đang đi chơi với nhau.",
      "spa": "Ellos están saliendo."
    },
    "text": "彼らは付き合っている。"
  },
  {
    "id": "3366851",
    "transcription": "[医者|い|しゃ]たちはトムに[言|い]った。メアリーに[回復|かい|ふく]の[見込|み|こ]みはないと。",
    "translations": {
      "eng": "The doctors told Tom that Mary would never recover.",
      "fin": "Lääkärit sanoivat Tomille, että ennusteiden mukaan Mari ei toivu."
    },
    "text": "医者たちはトムに言った。メアリーに回復の見込みはないと。"
  },
  {
    "id": "3366874",
    "transcription": "[僕|ぼく]はそんなに[勇敢|ゆう|かん]じゃない。",
    "translations": {
      "eng": "I'm not so brave.",
      "heb": "אני לא כל כך אמיץ.",
      "yid": "איך בין נישט אַזוי העלדיש."
    },
    "text": "僕はそんなに勇敢じゃない。"
  },
  {
    "id": "3366875",
    "transcription": "[僕|ぼく]はトムとは[違|ちが]う。",
    "translations": {
      "rus": "Мы с Томом разные.",
      "deu": "Ich bin nicht so wie Tom.",
      "cmn": "我跟汤姆不一样。",
      "nld": "Ik ben niet zoals Tom.",
      "kor": "난 톰이랑 달라.",
      "spa": "Yo y Tom somos diferentes.",
      "npi": "म टम भन्दा फरक छु।"
    },
    "audio": {
      "id": "203641",
      "author": "huizi99"
    },
    "text": "僕はトムとは違う。"
  },
  {
    "id": "3366876",
    "transcription": "[僕|ぼく]はトムみたいな[人間|にん|げん]ではない。",
    "translations": {
      "eng": "I'm not like Tom."
    },
    "text": "僕はトムみたいな人間ではない。"
  },
  {
    "id": "3366877",
    "transcription": "[僕|ぼく]は[彼女|かの|じょ]の[好|この]みのタイプではない。",
    "translations": {
      "eng": "I'm not her type.",
      "deu": "Ich bin nicht ihr Typ."
    },
    "text": "僕は彼女の好みのタイプではない。"
  },
  {
    "id": "3366878",
    "transcription": "[私|わたし]は[会員|かい|いん]ではない。",
    "translations": {
      "eng": "I'm not a member.",
      "deu": "Ich bin kein Mitglied."
    },
    "text": "私は会員ではない。"
  },
  {
    "id": "3366879",
    "transcription": "[事務所|じ|む|しょ]にいます。",
    "translations": {
      "eng": "I'm at the office."
    },
    "text": "事務所にいます。"
  },
  {
    "id": "3366881",
    "transcription": "[僕|ぼく]はこれが[得意|とく|い]だ。",
    "translations": {
      "eng": "I'm good at this.",
      "rus": "Я в этом силён."
    },
    "text": "僕はこれが得意だ。"
  },
  {
    "id": "3366882",
    "transcription": "[FBI|エフビーアイ]のものですが。",
    "translations": {
      "eng": "I'm from the FBI.",
      "spa": "Soy del FBI."
    },
    "text": "FBIのものですが。"
  },
  {
    "id": "3366883",
    "transcription": "[今|いま]、パイを[焼|や]いてるとこ。",
    "translations": {
      "eng": "I'm baking a pie."
    },
    "text": "今、パイを焼いてるとこ。"
  },
  {
    "id": "3366884",
    "transcription": "[今|いま]、ホテル。",
    "translations": {
      "eng": "I'm at the hotel.",
      "vie": "Bây giờ tôi đang ở khách sạn."
    },
    "text": "今、ホテル。"
  },
  {
    "id": "3366885",
    "transcription": "[僕|ぼく]ももう[大人|おとな]だ。",
    "translations": {
      "fra": "Moi aussi je suis adulte, à présent.",
      "eng": "I'm an adult now as well.",
      "por": "Eu também já sou adulto."
    },
    "text": "僕ももう大人だ。"
  },
  {
    "id": "3366887",
    "transcription": "[頑張|がん|ば]ってみるよ。",
    "translations": {
      "eng": "I'll try my best.",
      "fin": "Yritän tehdä parhaani.",
      "deu": "Ich werde mir Mühe geben."
    },
    "text": "頑張ってみるよ。"
  },
  {
    "id": "3366892",
    "transcription": "[自分|じ|ぶん]の[勘定|かん|じょう]は[自分|じ|ぶん]で[払|はら]います。",
    "translations": {
      "eng": "I'll pay my bill."
    },
    "text": "自分の勘定は自分で払います。"
  },
  {
    "id": "3366894",
    "transcription": "トムを[探|さが]しに[行|い]ってきます。",
    "translations": {
      "eng": "I'll go find Tom.",
      "fin": "Menen etsimään Tomin.",
      "rus": "Схожу поищу Тома.",
      "spa": "Voy a buscar a Tom."
    },
    "text": "トムを探しに行ってきます。"
  },
  {
    "id": "3366895",
    "transcription": "[残|のこ]りは[僕|ぼく]がやるよ。",
    "translations": {
      "eng": "I'll do the rest.",
      "rus": "Остальное сделаю я.",
      "slk": "Ostatné urobím ja."
    },
    "text": "残りは僕がやるよ。"
  },
  {
    "id": "3366897",
    "transcription": "[僕|ぼく]は[罰|ばつ]を[受|う]けるだろう。",
    "translations": {
      "eng": "I'll be punished.",
      "vie": "Tôi sẽ bị trừng phạt.",
      "deu": "Ich werde wohl bestraft werden."
    },
    "text": "僕は罰を受けるだろう。"
  },
  {
    "id": "3366899",
    "transcription": "[僕|ぼく]がバックアップするよ。",
    "translations": {
      "eng": "I'll back you up.",
      "rus": "Я тебя поддержу."
    },
    "text": "僕がバックアップするよ。"
  },
  {
    "id": "3366904",
    "transcription": "[僕|ぼく]は[黙|だま]らないぞ。",
    "translations": {
      "eng": "I won't be quiet."
    },
    "text": "僕は黙らないぞ。"
  },
  {
    "id": "3366905",
    "transcription": "[知|し]ってたらよかったんだけど。",
    "translations": {
      "eng": "I wish I'd known.",
      "fin": "Voi kunpa olisin tiennyt."
    },
    "text": "知ってたらよかったんだけど。"
  },
  {
    "id": "3366906",
    "transcription": "わたしもかつては[若|わか]かった。",
    "translations": {
      "eng": "I was young once.",
      "spa": "Yo también fui joven hace tiempo.",
      "fin": "Minäkin olin kerran nuori."
    },
    "text": "わたしもかつては若かった。"
  },
  {
    "id": "3366908",
    "transcription": "[2003|にせんさん][年|ねん]、[私|わたし]は[13|じゅうさん][歳|さい]だった。",
    "translations": {
      "eng": "I was 13 in 2003."
    },
    "text": "2003年、私は13歳だった。"
  },
  {
    "id": "3366910",
    "transcription": "[外出|がい|しゅつ]したいな。",
    "translations": {
      "fra": "Je veux sortir."
    },
    "text": "外出したいな。"
  },
  {
    "id": "3366911",
    "transcription": "[真実|しん|じつ]が[知|し]りたい。",
    "translations": {
      "eng": "I want the truth."
    },
    "text": "真実が知りたい。"
  },
  {
    "id": "3366912",
    "transcription": "ちょっとばかり[助|たす]けが[欲|ほ]しい。",
    "translations": {
      "eng": "I want some help.",
      "fin": "Haluaisin vähän apua."
    },
    "text": "ちょっとばかり助けが欲しい。"
  },
  {
    "id": "3366914",
    "transcription": "[私|わたし]はトムを[待|ま]った。",
    "translations": {
      "eng": "I waited for Tom."
    },
    "text": "私はトムを待った。"
  },
  {
    "id": "3366915",
    "transcription": "[私|わたし]は[１|いち][時間|じ|かん][待|ま]った。",
    "translations": {
      "eng": "I waited an hour."
    },
    "text": "私は１時間待った。"
  }
]