[
  {
    "id": "3496853",
    "transcription": "[絶対|ぜっ|たい][投|な]げ[出|だ]したらダメ。",
    "translations": {
      "eng": "Never give up.",
      "cmn": "决不放弃。",
      "deu": "Schmeiß auf keinen Fall hin!"
    },
    "text": "絶対投げ出したらダメ。"
  },
  {
    "id": "3496855",
    "transcription": "[門|もん]を[開|あ]けなさい。",
    "translations": {
      "deu": "Öffnet das Tor!"
    },
    "text": "門を開けなさい。"
  },
  {
    "id": "3496856",
    "transcription": "[開門|かい|もん]しなさい。",
    "translations": {
      "eng": "Open the gate."
    },
    "text": "開門しなさい。"
  },
  {
    "id": "3496871",
    "transcription": "もう[一度|いち|ど][読|よ]んで。",
    "translations": {
      "eng": "Read it again.",
      "fin": "Lue se vielä kerran.",
      "deu": "Lies das noch einmal.",
      "pol": "Przeczytaj to raz jeszcze."
    },
    "text": "もう一度読んで。"
  },
  {
    "id": "3496874",
    "transcription": "[今|いま]すぐこれを[読|よ]んで。",
    "translations": {
      "eng": "Read this right away."
    },
    "text": "今すぐこれを読んで。"
  },
  {
    "id": "3496877",
    "transcription": "[賭|か]け[事|ごと]はもうやめなよ。",
    "translations": {
      "eng": "Quit gambling."
    },
    "text": "賭け事はもうやめなよ。"
  },
  {
    "id": "3496901",
    "transcription": "サイを[振|ふ]れ。",
    "translations": {
      "eng": "Roll the dice.",
      "spa": "¡Tira los dados!"
    },
    "text": "サイを振れ。"
  },
  {
    "id": "3496923",
    "transcription": "[何|なに]か[言|い]ってよ。",
    "translations": {
      "eng": "Say something.",
      "deu": "Sag doch mal was!",
      "pol": "Powiedz coś.",
      "slk": "Povedzte niečo."
    },
    "text": "何か言ってよ。"
  },
  {
    "id": "3496936",
    "transcription": "[本|ほん]を[閉|と]じて。",
    "translations": {
      "eng": "Shut the book.",
      "rus": "Закрой книгу.",
      "deu": "Schließ das Buch."
    },
    "text": "本を閉じて。"
  },
  {
    "id": "3496944",
    "transcription": "そこに[私|わたし]を[連|つ]れてって。",
    "translations": {
      "eng": "Take me there.",
      "deu": "Kannst du mich da hinbringen?"
    },
    "text": "そこに私を連れてって。"
  },
  {
    "id": "3497306",
    "transcription": "それあんま[関係|かん|けい]なくない？",
    "translations": {
      "deu": "Das hat doch nichts miteinander zu tun, oder?",
      "fra": "Ça a pas grand-chose à voir avec ça, si ?",
      "eng": "That's not really the issue, is it?"
    },
    "text": "それあんま関係なくない？"
  },
  {
    "id": "3498454",
    "transcription": "いつからフランス[語|ご][勉強|べん|きょう]してるの？",
    "translations": {
      "fin": "Milloin aloitit ranskan opiskelun?",
      "deu": "Seit wann lernst du schon Französisch?",
      "rus": "Ты давно изучаешь французский?",
      "fra": "Depuis quand tu apprends le français ?",
      "vie": "Bạn bắt đầu học tiếng Pháp từ lúc nào đấy?",
      "ara": "منذ متى و أنت تدرس اللغة الفرنسية ؟"
    },
    "audio": {
      "id": "215702",
      "author": "yomi"
    },
    "text": "いつからフランス語勉強してるの？"
  },
  {
    "id": "3501384",
    "transcription": "[言|い]い[訳|わけ]ばっかすんなよ。",
    "translations": {
      "eng": "Don't just keep making excuses.",
      "fra": "Arrête de toujours chercher des excuses.",
      "deu": "Hör auf dich dauernd rauszureden!"
    },
    "text": "言い訳ばっかすんなよ。"
  },
  {
    "id": "3501425",
    "transcription": "[二人|ふたり]ともくだらない[喧嘩|けん|か]はやめろよ。",
    "translations": {
      "fra": "Vous avez pas bientôt fini de vous chamailler pour un rien, tous les deux ?",
      "rus": "Вы, оба, хватит уже ссориться по пустякам.",
      "eng": "Would the two of you quit bickering?",
      "deu": "Könntet ihr beiden aufhören, euch wegen Kleinigkeiten zu streiten?"
    },
    "text": "二人ともくだらない喧嘩はやめろよ。"
  },
  {
    "id": "3502005",
    "transcription": "あんまり[期待|き|たい]しすぎない[方|ほう]がいいと[思|おも]うよ。",
    "translations": {
      "deu": "Ich denke, es ist besser, sich nicht zu viel Hoffnung zu machen."
    },
    "text": "あんまり期待しすぎない方がいいと思うよ。"
  },
  {
    "id": "3502092",
    "transcription": "[駅|えき]まで[一緒|いっ|しょ]に[歩|ある]きませんか？",
    "translations": {
      "eng": "Why don't you walk to the station with us?",
      "deu": "Magst du nicht zusammen mit mir zum Bahnhof gehen?",
      "fra": "Et si nous marchions ensemble jusqu’à la gare ?",
      "spa": "¿Caminamos juntos hasta la estación?",
      "ind": "Maukah Anda jalan bersama sampai stasiun?"
    },
    "audio": {
      "id": "215819",
      "author": "Mizu"
    },
    "text": "駅まで一緒に歩きませんか？"
  },
  {
    "id": "3502345",
    "transcription": "あんまり[期待|き|たい]しすぎないでね。",
    "translations": {
      "eng": "Don't get your hopes up too much.",
      "spa": "No esperes demasiado.",
      "fra": "Ne place pas trop d’espoir là-dedans.",
      "deu": "Mache dir nicht zu viele Hoffnungen!",
      "ind": "Jangan berharap terlalu banyak."
    },
    "text": "あんまり期待しすぎないでね。"
  },
  {
    "id": "3504005",
    "transcription": "メアリーってかわいいと[思|おも]う？",
    "translations": {
      "eng": "Do you think Mary is cute?",
      "deu": "Findest du Maria süß?",
      "rus": "Ты считаешь, Мэри хорошенькая?",
      "fra": "Tu la trouves mignonne, Marie ?",
      "fin": "Onko Mari sinun mielestäsi nätti?",
      "spa": "¿Piensas que Mary es bonita?",
      "por": "Você acha Mary bonita?",
      "ind": "Menurutmu Mary manis?"
    },
    "audio": {
      "id": "215880",
      "author": "yomi"
    },
    "text": "メアリーってかわいいと思う？"
  },
  {
    "id": "3506550",
    "transcription": "そんなの[無駄|む|だ]だよ。",
    "translations": {
      "rus": "Это бесполезно."
    },
    "text": "そんなの無駄だよ。"
  },
  {
    "id": "3506551",
    "transcription": "[素晴|す|ば]らしいですね。",
    "translations": {
      "eng": "It's wonderful."
    },
    "text": "素晴らしいですね。"
  },
  {
    "id": "3506553",
    "transcription": "[笑|わら]い[事|ごと]ではない。",
    "translations": {
      "eng": "It's no joke.",
      "ind": "Ini bukan bahan tertawaan."
    },
    "text": "笑い事ではない。"
  },
  {
    "id": "3506554",
    "transcription": "そう[悪|わる]くはない。",
    "translations": {
      "eng": "It isn't bad."
    },
    "text": "そう悪くはない。"
  },
  {
    "id": "3506555",
    "transcription": "[完璧|かん|ぺき]だ。",
    "translations": {
      "fra": "Parfait.",
      "fin": "Se on täydellistä.",
      "deu": "Es ist perfekt.",
      "pol": "Jest bez zarzutu."
    },
    "text": "完璧だ。"
  },
  {
    "id": "3506556",
    "transcription": "[見|み]るからに[明|あき]らかだよ。",
    "translations": {
      "fra": "Ça saute aux yeux.",
      "eng": "It's obvious."
    },
    "text": "見るからに明らかだよ。"
  },
  {
    "id": "3506561",
    "transcription": "ずいぶんと[暗|くら]いな。",
    "translations": {
      "deu": "Es ist sehr dunkel.",
      "eng": "It is very dark."
    },
    "text": "ずいぶんと暗いな。"
  },
  {
    "id": "3506562",
    "transcription": "[難|むずか]しいわ。",
    "translations": {
      "eng": "It's so hard.",
      "cmn": "太难了。",
      "fin": "Tää on nii vaikeeta!",
      "pol": "To trudne."
    },
    "text": "難しいわ。"
  },
  {
    "id": "3506563",
    "transcription": "ずいぶんと[珍|めずら]しいですね。",
    "translations": {
      "eng": "It's so rare."
    },
    "text": "ずいぶんと珍しいですね。"
  },
  {
    "id": "3506566",
    "transcription": "[遠|とお]すぎるわ。",
    "translations": {
      "fra": "C'est trop loin.",
      "eng": "It's too far."
    },
    "text": "遠すぎるわ。"
  },
  {
    "id": "3506569",
    "transcription": "トムに[聞|き]いてごらん。",
    "translations": {
      "eng": "Ask Tom about it.",
      "ita": "Lo chieda a Tom."
    },
    "audio": {
      "id": "751026",
      "author": "lowteq"
    },
    "text": "トムに聞いてごらん。"
  },
  {
    "id": "3506570",
    "transcription": "まあ[落|お]ち[着|つ]け。",
    "translations": {
      "tur": "Sadece sakin ol.",
      "eng": "OK, calm down.",
      "rus": "Так, успокойся."
    },
    "text": "まあ落ち着け。"
  },
  {
    "id": "3506576",
    "transcription": "ちょっと[手伝|て|つだ]って。",
    "translations": {
      "eng": "Just help me.",
      "fin": "Auttaisitko vähän."
    },
    "text": "ちょっと手伝って。"
  },
  {
    "id": "3506579",
    "transcription": "うんと[言|い]ってよ。",
    "translations": {
      "eng": "Just say yes."
    },
    "text": "うんと言ってよ。"
  },
  {
    "id": "3506580",
    "transcription": "いいから[黙|だま]って。",
    "translations": {
      "eng": "Just shut up."
    },
    "text": "いいから黙って。"
  },
  {
    "id": "3506585",
    "transcription": "[踊|おど]り[続|つづ]けて。",
    "translations": {
      "eng": "Keep dancing.",
      "deu": "Tanzt weiter.",
      "fra": "Continue de danser.",
      "kor": "계속 춤 춰."
    },
    "text": "踊り続けて。"
  },
  {
    "id": "3506589",
    "transcription": "[歌|うた]い[続|つづ]けて。",
    "translations": {
      "eng": "Keep singing.",
      "deu": "Singt weiter."
    },
    "text": "歌い続けて。"
  },
  {
    "id": "3506593",
    "transcription": "[笑顔|え|がお]を[絶|た]やさないで。",
    "translations": {
      "eng": "Keep smiling.",
      "ita": "Non smettere di sorridere."
    },
    "text": "笑顔を絶やさないで。"
  },
  {
    "id": "3506602",
    "transcription": "トム、[遅|おく]れないでついてきて。",
    "translations": {
      "eng": "Keep up, Tom.",
      "rus": "Не отставай, Том."
    },
    "text": "トム、遅れないでついてきて。"
  },
  {
    "id": "3506603",
    "transcription": "[歩|ある]き[続|つづ]けて。",
    "translations": {
      "eng": "Keep walking."
    },
    "text": "歩き続けて。"
  },
  {
    "id": "3506604",
    "transcription": "[働|はたら]き[続|つづ]けなさい。",
    "translations": {
      "eng": "Keep on working.",
      "epo": "Daŭrigu labori.",
      "deu": "Arbeite weiter!"
    },
    "text": "働き続けなさい。"
  },
  {
    "id": "3506608",
    "transcription": "[私|わたし]にやらせてください。",
    "translations": {
      "eng": "Please let me do it."
    },
    "text": "私にやらせてください。"
  },
  {
    "id": "3506609",
    "transcription": "ちょっと[考|かんが]えさせて。",
    "translations": {
      "eng": "Let me think.",
      "cmn": "讓我想一想。"
    },
    "text": "ちょっと考えさせて。"
  },
  {
    "id": "3506612",
    "transcription": "トムに[知|し]らせろ。",
    "translations": {
      "eng": "Let Tom know."
    },
    "text": "トムに知らせろ。"
  },
  {
    "id": "3506617",
    "transcription": "トムを[休|やす]ませてやりなさい。",
    "translations": {
      "eng": "Let Tom rest."
    },
    "text": "トムを休ませてやりなさい。"
  },
  {
    "id": "3506619",
    "transcription": "そろそろ[行|い]きましょうか。",
    "translations": {
      "eng": "Let's go now."
    },
    "text": "そろそろ行きましょうか。"
  },
  {
    "id": "3506621",
    "transcription": "さあ[出|で]かけよう。",
    "translations": {
      "spa": "¡Venga, vámonos!",
      "eng": "Let's go out.",
      "deu": "Los, gehen wir!"
    },
    "text": "さあ出かけよう。"
  },
  {
    "id": "3506622",
    "transcription": "[行|い]くのはやめよう。",
    "translations": {
      "eng": "Let's not go.",
      "pol": "Nie idźmy."
    },
    "text": "行くのはやめよう。"
  },
  {
    "id": "3506636",
    "transcription": "[人生|じん|せい]は[苦|く]なり。",
    "translations": {
      "eng": "Life is difficult.",
      "deu": "Das Leben ist hart.",
      "cmn": "生活是难的。"
    },
    "text": "人生は苦なり。"
  },
  {
    "id": "3506637",
    "transcription": "あれを[見|み]て！",
    "translations": {
      "eng": "Look at that!",
      "fin": "Katso tuota!",
      "pol": "Popatrz na to!",
      "mfa": "taengor tuh!"
    },
    "text": "あれを見て！"
  },
  {
    "id": "3506638",
    "transcription": "これを[見|み]て。",
    "translations": {
      "eng": "Watch this.",
      "fin": "Katso tätä.",
      "fra": "Regarde ça.",
      "vie": "Hãy nhìn cái này."
    },
    "text": "これを見て。"
  },
  {
    "id": "3506639",
    "transcription": "よく[見|み]て。",
    "translations": {
      "eng": "Look closely.",
      "pol": "Przyjrzyj się uważnie.",
      "fra": "Regarde bien.",
      "ita": "Guarda bene!"
    },
    "text": "よく見て。"
  }
]