  },
  {
    "id": "3480428",
    "transcription": "トムを[捕|つか]まえろ。",
    "translations": {
      "eng": "Grab Tom.",
      "ind": "Tangkap Tom.",
      "rus": "Схвати Тома."
    },
    "audio": {
      "id": "751023",
      "author": "lowteq"
    },
    "text": "トムを捕まえろ。"
  },
  {
    "id": "3480431",
    "transcription": "トムを[助|たす]けてやって。",
    "translations": {
      "eng": "Help Tom.",
      "rus": "Помогите Тому.",
      "uig": "تومغا ياردەم قىل."
    },
    "audio": {
      "id": "751024",
      "author": "lowteq"
    },
    "text": "トムを助けてやって。"
  },
  {
    "id": "3480582",
    "transcription": "[道|みち]に[迷|まよ]ってしまった。",
    "translations": {
      "eng": "I am lost.",
      "deu": "Ich habe mich verlaufen.",
      "cmn": "我迷失了。",
      "fin": "Olen eksynyt."
    },
    "text": "道に迷ってしまった。"
  },
  {
    "id": "3480930",
    "transcription": "[僕|ぼく]は[君|きみ]を[好|す]きになり[始|はじ]めている。",
    "translations": {
      "eng": "I'm starting to like you."
    },
    "text": "僕は君を好きになり始めている。"
  },
  {
    "id": "3480954",
