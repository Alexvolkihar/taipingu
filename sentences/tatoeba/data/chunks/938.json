[
  {
    "id": "3506644",
    "transcription": "[当|あ]ててみて。",
    "translations": {
      "eng": "Try to guess.",
      "deu": "Rate mal!",
      "ind": "Coba tebak."
    },
    "text": "当ててみて。"
  },
  {
    "id": "3506675",
    "transcription": "あごが[痛|いた]い。",
    "translations": {
      "eng": "My jaw is sore.",
      "fra": "J'ai mal au menton."
    },
    "text": "あごが痛い。"
  },
  {
    "id": "3506676",
    "transcription": "[誰|だれ]も[気|き]にしやしないよ。",
    "translations": {
      "eng": "No one will care.",
      "spa": "A nadie le va a importar."
    },
    "text": "誰も気にしやしないよ。"
  },
  {
    "id": "3506677",
    "transcription": "[誰|だれ]も[口|くち]を[開|ひら]かなかった。",
    "translations": {
      "eng": "No one spoke.",
      "deu": "Es hat niemand etwas gesagt.",
      "pol": "Nikt nic nie mówił."
    },
    "text": "誰も口を開かなかった。"
  },
  {
    "id": "3506678",
    "transcription": "さあ[飲|の]み[干|ほ]して。",
    "translations": {
      "eng": "Now drink up."
    },
    "text": "さあ飲み干して。"
  },
  {
    "id": "3506679",
    "transcription": "さあ[楽|たの]しんで。",
    "translations": {
      "rus": "Развлекайтесь.",
      "eng": "Now have fun."
    },
    "text": "さあ楽しんで。"
  },
  {
    "id": "3506681",
    "transcription": "さあ[横|よこ]になって。",
    "translations": {
      "eng": "Now lie down.",
      "spa": "Vamos, túmbate.",
      "fin": "No niin, menehän makuulle."
    },
    "text": "さあ横になって。"
  },
  {
    "id": "3506685",
    "transcription": "[頼|たの]むから、[急|いそ]いで！",
    "translations": {
      "eng": "Please hurry!"
    },
    "text": "頼むから、急いで！"
  },
  {
    "id": "3506692",
    "transcription": "[物静|もの|しず]かな[人|ひと]だな。",
    "translations": {
      "eng": "He is quiet."
    },
    "text": "物静かな人だな。"
  },
  {
    "id": "3506693",
    "transcription": "あの[子|こ]はまだ[若|わか]い。",
    "translations": {
      "eng": "She is young."
    },
    "text": "あの子はまだ若い。"
  },
  {
    "id": "3507321",
    "transcription": "ガスの[臭|にお]いがする。",
    "translations": {
      "eng": "I smell gas.",
      "cmn": "我聞到瓦斯味。",
      "fin": "Haisee kaasulle."
    },
    "text": "ガスの臭いがする。"
  },
  {
    "id": "3507336",
    "transcription": "[泣|な]くもんか。",
    "translations": {
      "eng": "I won't cry."
    },
    "text": "泣くもんか。"
  },
  {
    "id": "3507341",
    "transcription": "[死|し]ぬもんか。",
    "translations": {
      "eng": "I will not die.",
      "deu": "Ich sterbe doch nicht.",
      "fra": "Je ne mourrai pas."
    },
    "text": "死ぬもんか。"
  },
  {
    "id": "3507342",
    "transcription": "[嘘|うそ]などつくもんか。",
    "translations": {
      "eng": "I won't lie.",
      "deu": "Ich lüge doch nicht."
    },
    "text": "嘘などつくもんか。"
  },
  {
    "id": "3507350",
    "transcription": "[俺|おれ]は[気|き]が[小|ちい]さい。",
    "translations": {
      "rus": "Я трусливый."
    },
    "text": "俺は気が小さい。"
  },
  {
    "id": "3507354",
    "transcription": "[僕|ぼく]は[意気地|い|く|じ]なしだ。",
    "translations": {
      "eng": "I'm chicken.",
      "deu": "Ich bin ein Feigling."
    },
    "text": "僕は意気地なしだ。"
  },
  {
    "id": "3507377",
    "transcription": "[私|わたし]は[英雄|えい|ゆう]などではない。",
    "translations": {
      "vie": "Tôi chẳng phải anh hùng hay gì đâu.",
      "fin": "Minä en ole mikään sankari."
    },
    "text": "私は英雄などではない。"
  },
  {
    "id": "3507378",
    "transcription": "[私|わたし]、[嘘|うそ]つきじゃないもん。",
    "translations": {
      "eng": "I am not a liar.",
      "deu": "Ich bin kein Lügner.",
      "vie": "Tôi không phải là đồ nói dối."
    },
    "text": "私、嘘つきじゃないもん。"
  },
  {
    "id": "3507380",
    "transcription": "[私|わたし]は[内気|うち|き]ではない。",
    "translations": {
      "eng": "I am not shy.",
      "fra": "Je ne suis pas timide.",
      "deu": "Ich bin nicht schüchtern."
    },
    "text": "私は内気ではない。"
  },
  {
    "id": "3507381",
    "transcription": "[私|わたし]って[自己|じ|こ][中|ちゅう]だな。",
    "translations": {
      "eng": "I'm selfish.",
      "deu": "Ich bin selbstsüchtig."
    },
    "text": "私って自己中だな。"
  },
  {
    "id": "3507382",
    "transcription": "[私|わたし]がここの[最高|さい|こう][責任|せき|にん][者|しゃ]です。",
    "translations": {
      "eng": "I'm the CEO."
    },
    "text": "私がここの最高責任者です。"
  },
  {
    "id": "3507384",
    "transcription": "[閉|と]じ[込|こ]められてしまった。",
    "translations": {
      "eng": "I'm trapped."
    },
    "text": "閉じ込められてしまった。"
  },
  {
    "id": "3507387",
    "transcription": "[俺|おれ]って[役立|やく|た]たずだな。",
    "translations": {
      "eng": "I'm useless.",
      "deu": "Da habe ich wohl nicht geholfen."
    },
    "text": "俺って役立たずだな。"
  },
  {
    "id": "3507395",
    "transcription": "[分|わ]かりますか？",
    "translations": {
      "eng": "Do you understand it?",
      "deu": "Verstehst du das?",
      "fin": "Tajuatko?",
      "ryu": "わかやびーみ？"
    },
    "text": "分かりますか？"
  },
  {
    "id": "3507399",
    "transcription": "[霧|きり]が[出|で]ていますか？",
    "translations": {
      "deu": "Ist es neblig?"
    },
    "text": "霧が出ていますか？"
  },
  {
    "id": "3507400",
    "transcription": "[霧|きり]は[出|で]ていますか？",
    "translations": {
      "eng": "Is it foggy?"
    },
    "text": "霧は出ていますか？"
  },
  {
    "id": "3507409",
    "transcription": "それで[全部|ぜん|ぶ]？",
    "translations": {
      "eng": "That's all?",
      "pol": "To wszystko?"
    },
    "text": "それで全部？"
  },
  {
    "id": "3507422",
    "transcription": "トムは[無事|ぶ|じ]だろうか。",
    "translations": {
      "eng": "Is Tom safe?"
    },
    "text": "トムは無事だろうか。"
  },
  {
    "id": "3507424",
    "transcription": "トムは[病気|びょう|き]なの？",
    "translations": {
      "eng": "Is Tom sick?",
      "rus": "Том болен?",
      "deu": "Ist Tom krank?",
      "fin": "Onks Tom kipeen?",
      "cmn": "汤姆生病了吗？",
      "ind": "Apa Tom sakit?"
    },
    "audio": {
      "id": "216043",
      "author": "huizi99"
    },
    "text": "トムは病気なの？"
  },
  {
    "id": "3507428",
    "transcription": "[寒|さむ]かった。",
    "translations": {
      "eng": "It was cold.",
      "pol": "Było zimno.",
      "fin": "Oli kylmä.",
      "slk": "Bolo chladno.",
      "fra": "Il faisait froid."
    },
    "text": "寒かった。"
  },
  {
    "id": "3507430",
    "transcription": "[暗|くら]かった。",
    "translations": {
      "eng": "It was dark.",
      "jbo": "pu manku",
      "fin": "Oli pimeää.",
      "fra": "Il faisait sombre.",
      "pol": "Było ciemno.",
      "spa": "Estaba oscuro."
    },
    "text": "暗かった。"
  },
  {
    "id": "3507431",
    "transcription": "[簡単|かん|たん]だった。",
    "translations": {
      "eng": "It was easy.",
      "fra": "C'était facile."
    },
    "text": "簡単だった。"
  },
  {
    "id": "3507437",
    "transcription": "それは[壊|こわ]れている。",
    "translations": {
      "eng": "That's broken.",
      "fin": "Tuo on rikki.",
      "kor": "그것은 망가져있다."
    },
    "text": "それは壊れている。"
  },
  {
    "id": "3507438",
    "transcription": "[皮肉|ひ|にく]だな。",
    "translations": {
      "eng": "It's ironic.",
      "fra": "C'est ironique.",
      "deu": "Das ist Ironie.",
      "pol": "To zakrawa na ironię.",
      "fin": "Aika ironista.",
      "spa": "Es irónico."
    },
    "text": "皮肉だな。"
  },
  {
    "id": "3507439",
    "transcription": "[鍵|かぎ]がかかっている。",
    "translations": {
      "eng": "It's locked.",
      "deu": "Es ist abgeschlossen."
    },
    "text": "鍵がかかっている。"
  },
  {
    "id": "3507444",
    "transcription": "[随分|ずい|ぶん]と[大|おお]きいな。",
    "translations": {
      "eng": "It's so big.",
      "deu": "Das ist ganz schön groß!"
    },
    "text": "随分と大きいな。"
  },
  {
    "id": "3507448",
    "transcription": "[緊急|きん|きゅう]です。",
    "translations": {
      "eng": "It's urgent.",
      "fra": "C'est urgent.",
      "pol": "To pilne.",
      "fin": "Tämä on kiireellistä.",
      "nld": "Het is dringend."
    },
    "text": "緊急です。"
  },
  {
    "id": "3507452",
    "transcription": "[私|わたし]に[知|し]らせて。",
    "translations": {
      "deu": "Lass es mich wissen."
    },
    "text": "私に知らせて。"
  },
  {
    "id": "3507453",
    "transcription": "ご[一報|いっ|ぽう]ください。",
    "translations": {
      "eng": "Let me know."
    },
    "text": "ご一報ください。"
  },
  {
    "id": "3507455",
    "transcription": "さあ[踊|おど]ろう。",
    "translations": {
      "eng": "Let's dance.",
      "epo": "Ni dancu.",
      "nld": "Laten we dansen."
    },
    "text": "さあ踊ろう。"
  },
  {
    "id": "3507456",
    "transcription": "さあ[飲|の]もうじゃないか。",
    "translations": {
      "eng": "Let's drink."
    },
    "text": "さあ飲もうじゃないか。"
  },
  {
    "id": "3507460",
    "transcription": "さあ、みんなで[盛|も]り[上|あ]がろう！",
    "translations": {
      "eng": "Let's party!"
    },
    "text": "さあ、みんなで盛り上がろう！"
  },
  {
    "id": "3507466",
    "transcription": "トムを[見|み]てごらん。",
    "translations": {
      "eng": "Look at Tom.",
      "rus": "Посмотри на Тома.",
      "deu": "Schau dir Tom an.",
      "nld": "Kijk naar Tom."
    },
    "audio": {
      "id": "216044",
      "author": "huizi99"
    },
    "text": "トムを見てごらん。"
  },
  {
    "id": "3507468",
    "transcription": "[願|ねが]い[事|ごと]をして。",
    "translations": {
      "eng": "Make a wish.",
      "fin": "Toivo jotain.",
      "cmn": "许个愿吧。"
    },
    "text": "願い事をして。"
  },
  {
    "id": "3507470",
    "transcription": "さあ[家|いえ]に[帰|かえ]りなさい。",
    "translations": {
      "eng": "Now go on home.",
      "fin": "No niin, mene kotiin."
    },
    "text": "さあ家に帰りなさい。"
  },
  {
    "id": "3507479",
    "transcription": "[一|ひと]つは[青|あお]。",
    "translations": {
      "eng": "One is blue.",
      "fin": "Yksi on sininen.",
      "pol": "Jeden jest niebieski.",
      "mfa": "satu tuh bighu"
    },
    "text": "一つは青。"
  },
  {
    "id": "3507487",
    "transcription": "[落|お]ち[着|つ]いて！",
    "translations": {
      "eng": "Settle down!",
      "pol": "Uspokój się!"
    },
    "text": "落ち着いて！"
  },
  {
    "id": "3507492",
    "transcription": "あの[女|おんな]は[不細工|ぶ|さい|く]だ。",
    "translations": {
      "eng": "She's a dog.",
      "fin": "Hän on rumilus."
    },
    "text": "あの女は不細工だ。"
  },
  {
    "id": "3507493",
    "transcription": "どうやってやるのか[教|おし]えて。",
    "translations": {
      "eng": "Show me how.",
      "ita": "Insegnami come fare."
    },
    "text": "どうやってやるのか教えて。"
  },
  {
    "id": "3507496",
    "transcription": "[寝|ね]てなさい。",
    "translations": {
      "eng": "Stay in bed."
    },
    "text": "寝てなさい。"
  }
]