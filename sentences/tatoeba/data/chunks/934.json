[
  {
    "id": "3484383",
    "transcription": "[宿題|しゅく|だい]って[何|なん]のためにあるんだろう。",
    "translations": {
      "nob": "Hva skal lekser være godt for?",
      "fra": "Mais à quoi servent donc les devoirs ?",
      "rus": "И зачем только вообще нужны уроки?",
      "fin": "Mitäköhän varten läksyt ovat olemassa?",
      "deu": "Warum muss es nur Hausaufgaben geben?",
      "eng": "Why do we have homework?",
      "spa": "¿Para qué tendremos deberes?"
    },
    "audio": {
      "id": "215273",
      "author": "yomi"
    },
    "text": "宿題って何のためにあるんだろう。"
  },
  {
    "id": "3485202",
    "transcription": "それは[愚問|ぐ|もん]ですよ。",
    "translations": {
      "eng": "That's a stupid question.",
      "cmn": "这是一个愚蠢的问题。",
      "rus": "Это глупый вопрос.",
      "nob": "Det er et dumt spørsmål!"
    },
    "text": "それは愚問ですよ。"
  },
  {
    "id": "3486249",
    "transcription": "お[支払|し|はら]いはいかがなさいますか？",
    "translations": {
      "eng": "How would you like to pay?",
      "rus": "Как будете оплачивать?",
      "pol": "Jak chce pan zapłacić?",
      "deu": "Wie möchten Sie zahlen?",
      "spa": "¿Cómo le gustaría pagar?"
    },
    "audio": {
      "id": "215317",
      "author": "Mizu"
    },
    "text": "お支払いはいかがなさいますか？"
  },
  {
    "id": "3486602",
    "transcription": "[運転|うん|てん][免許|めん|きょ]はお[持|も]ちではないでしょうか。",
    "translations": {
      "eng": "Don't you have a driver's license?"
    },
    "text": "運転免許はお持ちではないでしょうか。"
  },
  {
    "id": "3487062",
    "transcription": "トムなんか[雷|かみなり]に[打|う]たれて[死|し]ねばいいのに。",
    "translations": {
      "deu": "Tom könnte meinetwegen vom Blitz erschlagen werden.",
      "eng": "I wish Tom would get struck by lightning and die."
    },
    "text": "トムなんか雷に打たれて死ねばいいのに。"
  },
  {
    "id": "3487245",
    "transcription": "これ[何|なん][回|かい][見|み]ても[笑|わら]える。",
    "translations": {
      "deu": "Das bringt mich jedesmal zum Lachen.",
      "eng": "No matter how many times you see this, you still laugh."
    },
    "text": "これ何回見ても笑える。"
  },
  {
    "id": "3488005",
    "transcription": "[起立|き|りつ]！",
    "translations": {
      "eng": "All rise!",
      "pol": "Wstawaj!",
      "spa": "¡De pie!",
      "ind": "Berdiri!",
      "eus": "Zutik!",
      "yue": "起立！",
      "nob": "Reis dere!",
      "cmn": "起立！"
    },
    "text": "起立！"
  },
  {
    "id": "3488008",
    "transcription": "[戻|もど]っておいで。",
    "translations": {
      "eng": "Come back in.",
      "cmn": "请回来！",
      "mkd": "Врати се.",
      "ces": "Vraťte se.",
      "srp": "Врати се.",
      "rus": "Вернись.",
      "slk": "Vráťte sa."
    },
    "audio": {
      "id": "753966",
      "author": "lowteq"
    },
    "text": "戻っておいで。"
  },
  {
    "id": "3488013",
    "transcription": "[嘘|うそ]つけ！",
    "translations": {
      "eng": "Liar!",
      "spa": "¡Mentirosa!",
      "pol": "Ty kłamco!",
      "fra": "Menteur !",
      "fin": "Valehtelija!",
      "rus": "Врун!",
      "ain": "sunkep e-ne ruwe!"
    },
    "text": "嘘つけ！"
  },
  {
    "id": "3488029",
    "transcription": "もう[寝|ね]なさい。",
    "translations": {
      "eng": "You need to go to bed.",
      "fin": "Mene sänkyyn!",
      "vie": "Đừng thức nữa, đi ngủ đi."
    },
    "audio": {
      "id": "753968",
      "author": "lowteq"
    },
    "text": "もう寝なさい。"
  },
  {
    "id": "3488050",
    "transcription": "ダメだって[言|い]ったでしょ。",
    "translations": {
      "deu": "Ich sagte nein.",
      "rus": "Я же сказала нет.",
      "ind": "Sudah dibilang jangan."
    },
    "audio": {
      "id": "753969",
      "author": "lowteq"
    },
    "text": "ダメだって言ったでしょ。"
  },
  {
    "id": "3488053",
    "transcription": "[違|ちが]うってば。",
    "translations": {
      "eng": "I said no.",
      "rus": "Я сказал нет.",
      "khm": "ខ្ញុំប្រាប់ហើយថាមិនមែនទេ។"
    },
    "audio": {
      "id": "753970",
      "author": "lowteq"
    },
    "text": "違うってば。"
  },
  {
    "id": "3488056",
    "transcription": "そう[言|い]っておいたはずだ。",
    "translations": {
      "eng": "I said so.",
      "deu": "Das hatte ich dir eigentlich gesagt.",
      "ind": "Aku kan sudah bilang begitu tadi."
    },
    "audio": {
      "id": "753971",
      "author": "lowteq"
    },
    "text": "そう言っておいたはずだ。"
  },
  {
    "id": "3488059",
    "transcription": "これが[欲|ほ]しい。",
    "translations": {
      "eng": "I want these.",
      "ind": "Aku mau ini."
    },
    "text": "これが欲しい。"
  },
  {
    "id": "3488083",
    "transcription": "そこって[遠|とお]いですか？",
    "translations": {
      "eng": "Is it far?",
      "fin": "Onko se kaukana?",
      "fra": "C’est loin ?",
      "tur": "Orası uzak mı? (O dediğin yer uzak mıdır?)",
      "slk": "Je to ďaleko?",
      "ceb": "Layo na?"
    },
    "audio": {
      "id": "753972",
      "author": "lowteq"
    },
    "text": "そこって遠いですか？"
  },
  {
    "id": "3488089",
    "transcription": "[動|うご]いた！",
    "translations": {
      "dtp": "Poimpanau dii!",
      "pol": "Zadziałało!"
    },
    "text": "動いた！"
  },
  {
    "id": "3488093",
    "transcription": "うまく[行|い]った！",
    "translations": {
      "eng": "It worked.",
      "spa": "Ha ido bien."
    },
    "text": "うまく行った！"
  },
  {
    "id": "3488102",
    "transcription": "それ、[私|わたし]の。",
    "translations": {
      "rus": "Это моё."
    },
    "audio": {
      "id": "753973",
      "author": "lowteq"
    },
    "text": "それ、私の。"
  },
  {
    "id": "3488104",
    "transcription": "それ、[私|わたし]のです。",
    "translations": {
      "eng": "That's mine.",
      "deu": "Das gehört mir.",
      "tok": "ni li ijo mi.",
      "nob": "Det er mitt.",
      "cmn": "那是我的。",
      "slk": "To je moje."
    },
    "audio": {
      "id": "753974",
      "author": "lowteq"
    },
    "text": "それ、私のです。"
  },
  {
    "id": "3488115",
    "transcription": "[大丈夫|だい|じょう|ぶ]、[大丈夫|だい|じょう|ぶ]。",
    "translations": {
      "eng": "It's all right!",
      "deu": "Es ist in Ordnung."
    },
    "audio": {
      "id": "753975",
      "author": "lowteq"
    },
    "text": "大丈夫、大丈夫。"
  },
  {
    "id": "3488126",
    "transcription": "[下|さ]がってて。",
    "translations": {
      "eng": "Keep back.",
      "spa": "Baja.",
      "nld": "Kom naar beneden.",
      "vie": "Lùi lại đi."
    },
    "audio": {
      "id": "753976",
      "author": "lowteq"
    },
    "text": "下がってて。"
  },
  {
    "id": "3488129",
    "transcription": "[離|はな]れてて。",
    "translations": {
      "fin": "Älä tule lähemmäs!",
      "spa": "Sepárate."
    },
    "audio": {
      "id": "753977",
      "author": "lowteq"
    },
    "text": "離れてて。"
  },
  {
    "id": "3488157",
    "transcription": "そこにお[座|すわ]り。",
    "translations": {
      "eng": "Sit there.",
      "vie": "Xin mời ngồi chỗ kia.",
      "ain": "toanta rok yan.",
      "fin": "Istu siihen."
    },
    "audio": {
      "id": "753979",
      "author": "lowteq"
    },
    "text": "そこにお座り。"
  },
  {
    "id": "3488162",
    "transcription": "[近|ちか]づくな！",
    "translations": {
      "eng": "Keep away.",
      "deu": "Komm nicht näher!",
      "spa": "¡No se acerque!",
      "ind": "Jangan mendekat!",
      "pol": "Nie podchodź!",
      "epo": "Restu for.",
      "ita": "Non ti avvicinare!"
    },
    "text": "近づくな！"
  },
  {
    "id": "3488166",
    "transcription": "[下|さ]がって。",
    "translations": {
      "eng": "Step back."
    },
    "audio": {
      "id": "753981",
      "author": "lowteq"
    },
    "text": "下がって。"
  },
  {
    "id": "3488189",
    "transcription": "[上出来|じょう|で|き]！",
    "translations": {
      "eng": "Well done!",
      "pol": "Świetny wynik!",
      "fin": "Hienoa työtä!",
      "ind": "Kerja bagus!"
    },
    "text": "上出来！"
  },
  {
    "id": "3488208",
    "transcription": "[私|わたし]の[言|い]ってること、わかります？",
    "translations": {
      "eng": "Am I clear?",
      "fra": "Est-ce que tu comprends ce que je dis ?"
    },
    "text": "私の言ってること、わかります？"
  },
  {
    "id": "3488251",
    "transcription": "[私|わたし]、[正|ただ]しい？",
    "translations": {
      "eng": "Am I correct?",
      "pol": "Mam rację?"
    },
    "text": "私、正しい？"
  },
  {
    "id": "3488253",
    "transcription": "[時間|じ|かん]に[遅|おく]れないで。",
    "translations": {
      "eng": "Be on time.",
      "pol": "Nie spóźnię się.",
      "deu": "Sei pünktlich."
    },
    "text": "時間に遅れないで。"
  },
  {
    "id": "3488265",
    "transcription": "[辛抱|しん|ぼう]しなさい。",
    "translations": {
      "eng": "Be patient.",
      "deu": "Sei geduldig!",
      "fra": "Sois patient.",
      "fin": "Kärsivällisyyttä."
    },
    "text": "辛抱しなさい。"
  },
  {
    "id": "3488268",
    "transcription": "[真面目|ま|じ|め]におやり。",
    "translations": {
      "eng": "Be serious.",
      "deu": "Sei ernst."
    },
    "text": "真面目におやり。"
  },
  {
    "id": "3488270",
    "transcription": "[私|わたし]も[行|い]ってもいい？",
    "translations": {
      "eng": "Can I tag along?",
      "spa": "¿Qué tal si voy yo también?",
      "cmn": "我可以去吗？"
    },
    "text": "私も行ってもいい？"
  },
  {
    "id": "3488271",
    "transcription": "これ[運|はこ]んで。",
    "translations": {
      "eng": "Carry this.",
      "deu": "Trag das."
    },
    "text": "これ運んで。"
  },
  {
    "id": "3488273",
    "transcription": "[一|ひと]つ[選|えら]んで。",
    "translations": {
      "cmn": "挑选一个吧。",
      "eng": "Choose one.",
      "deu": "Suche eines aus.",
      "por": "Escolha um.",
      "spa": "Escoge uno.",
      "rus": "Выберите одно.",
      "mkd": "Избери едно.",
      "ber": "Fernemt yiwen.",
      "srp": "Изаберите једно.",
      "pol": "Wybierz jeden.",
      "slk": "Vyberte jedno.",
      "mfa": "bileh sute"
    },
    "text": "一つ選んで。"
  },
  {
    "id": "3488274",
    "transcription": "また[来|き]てね。",
    "translations": {
      "eng": "Come again.",
      "kor": "다시 와."
    },
    "text": "また来てね。"
  },
  {
    "id": "3488276",
    "transcription": "[見|み]ないで。",
    "translations": {
      "eng": "Don't look.",
      "ita": "Non guardare!",
      "pol": "Nie patrz.",
      "fin": "Älä kato!",
      "cmn": "別看。",
      "fra": "Ne regarde pas."
    },
    "text": "見ないで。"
  },
  {
    "id": "3488277",
    "transcription": "[動|うご]くな！",
    "translations": {
      "deu": "Keine Bewegung!",
      "epo": "Ne moviĝu!",
      "eng": "Keep still!",
      "rus": "Не двигайся!",
      "cmn": "别动！",
      "fin": "Liikkumatta!",
      "ita": "Non ti muovere!",
      "ind": "Jangan bergerak!",
      "nld": "Halt!",
      "pol": "Nie ruszaj się!"
    },
    "text": "動くな！"
  },
  {
    "id": "3488278",
    "transcription": "[押|お]すなよ。",
    "translations": {
      "eng": "Don't push.",
      "rus": "Не нажимай!",
      "fin": "Älä työnnä.",
      "pol": "Nie pchaj się!",
      "deu": "Nicht schubsen!",
      "cmn": "別推！"
    },
    "text": "押すなよ。"
  },
  {
    "id": "3488284",
    "transcription": "[来|こ]ないで。",
    "translations": {
      "eng": "Don't come.",
      "fra": "Ne viens pas.",
      "pol": "Nie przychodź.",
      "fin": "Älä tule.",
      "npi": "नआउ।"
    },
    "text": "来ないで。"
  },
  {
    "id": "3488290",
    "transcription": "[許|ゆる]して。",
    "translations": {
      "fra": "Pardonnez-moi.",
      "eng": "Forgive me.",
      "pol": "Wybacz mi.",
      "spa": "¡Perdóname!"
    },
    "text": "許して。"
  },
  {
    "id": "3488295",
    "transcription": "さあ[行|い]った、[行|い]った。",
    "translations": {
      "eng": "Get going."
    },
    "text": "さあ行った、行った。"
  },
  {
    "id": "3488297",
    "transcription": "[諦|あきら]めろ。",
    "translations": {
      "eng": "Give it up.",
      "fra": "Laisse tomber.",
      "spa": "Ríndanse.",
      "pol": "Poddaj się.",
      "nob": "Gi opp!",
      "fin": "Luovuta!"
    },
    "text": "諦めろ。"
  },
  {
    "id": "3488314",
    "transcription": "[賢|かしこ]い！",
    "translations": {
      "ara": "كم هو ذكي!",
      "eng": "How clever!",
      "pol": "Sprytnie!",
      "spa": "¡Muy astuto!",
      "hun": "Okos!",
      "fin": "Vähäks fiksuu!",
      "ind": "Cerdas!",
      "nob": "Du er smart!",
      "ita": "Brillante!",
      "khm": "ឆ្លាត!",
      "cmn": "聰明！"
    },
    "text": "賢い！"
  },
  {
    "id": "3488326",
    "transcription": "[僕|ぼく]、[男|おとこ]だよ。",
    "translations": {
      "epo": "Mi estas knabo.",
      "eng": "I'm a boy.",
      "spa": "Soy un niño.",
      "tok": "mi mije a.",
      "khm": "ខ្ញុំជាបុរសតើស។",
      "npi": "म केटा हुँ।",
      "ain": "okkayo ku-ne ruwe ne."
    },
    "audio": {
      "id": "753984",
      "author": "lowteq"
    },
    "text": "僕、男だよ。"
  },
  {
    "id": "3488334",
    "transcription": "[私|わたし]は[行|い]かれません。",
    "translations": {
      "fin": "En voi mennä."
    },
    "text": "私は行かれません。"
  },
  {
    "id": "3488339",
    "transcription": "[羨|うらや]ましい！",
    "translations": {
      "eng": "I envy them.",
      "spa": "¡Qué envidia!",
      "pol": "Zazdroszczę ci.",
      "fin": "Olen niin kateellinen!",
      "kor": "네가 부러워.",
      "ain": "aynupata!"
    },
    "text": "羨ましい！"
  },
  {
    "id": "3488341",
    "transcription": "[私|わたし]は[行|い]けません。",
    "translations": {
      "eng": "I can't go.",
      "rus": "Я не могу пойти.",
      "spa": "No puedo ir."
    },
    "text": "私は行けません。"
  },
  {
    "id": "3488360",
    "transcription": "[気|き]に[入|い]りました。",
    "translations": {
      "eng": "I liked it."
    },
    "text": "気に入りました。"
  },
  {
    "id": "3488397",
    "transcription": "[独身|どく|しん]です。",
    "translations": {
      "eng": "I'm single.",
      "pol": "Jestem singlem."
    },
    "text": "独身です。"
  },
  {
    "id": "3488400",
    "transcription": "あー、[眠|ねむ]い。",
    "translations": {
      "ind": "Ah, ngantuk."
    },
    "text": "あー、眠い。"
  }
]