[
  {
    "id": "9314689",
    "transcription": "お[釣|つ]りは[取|と]っといて！",
    "translations": {
      "eng": "Keep the change!"
    },
    "text": "お釣りは取っといて！"
  },
  {
    "id": "9314693",
    "transcription": "「いつ[戻|もど]ったの？」「[一昨日|おととい]だよ」",
    "translations": {
      "fin": "\"Milloin tulit takaisin?\" – \"Toissa päivänä.\""
    },
    "text": "「いつ戻ったの？」「一昨日だよ」"
  },
  {
    "id": "9314701",
    "transcription": "[今夜|こん|や]は[雨|あめ]になるでしょう。",
    "translations": {
      "eng": "It will rain tonight.",
      "deu": "Heute Nacht wird es regnen."
    },
    "text": "今夜は雨になるでしょう。"
  },
  {
    "id": "9314702",
    "transcription": "どうしてあなたに[謝|あやま]らないといけないの？",
    "translations": {
      "fra": "Pourquoi devrais-je vous présenter mes excuses ?"
    },
    "text": "どうしてあなたに謝らないといけないの？"
  },
  {
    "id": "9314711",
    "transcription": "「[戻|もど]ったのはいつなの？」「おととい[戻|もど]ったよ」",
    "translations": {
      "eng": "\"When did you return?\" \"I came back the day before yesterday.\""
    },
    "text": "「戻ったのはいつなの？」「おととい戻ったよ」"
  },
  {
    "id": "9314715",
    "transcription": "この[本|ほん]は[非売品|ひ|ばい|ひん]です。",
    "translations": {
      "eng": "This book isn't for sale."
    },
    "text": "この本は非売品です。"
  },
  {
    "id": "9314721",
    "transcription": "クラシックが[好|す]きなんでしょ？",
    "translations": {
      "eng": "You like classical music, don't you?",
      "cmn": "你喜歡古典音樂, 對不對?"
    },
    "text": "クラシックが好きなんでしょ？"
  },
  {
    "id": "9314730",
    "transcription": "このスープ、ちょっと[塩味|しお|あじ]が[足|た]りないかな。",
    "translations": {
      "eng": "I think the soup needs a bit of salt."
    },
    "text": "このスープ、ちょっと塩味が足りないかな。"
  },
  {
    "id": "9314739",
    "transcription": "[部屋|へ|や]の[掃除|そう|じ]は[娘|むすめ]の[役目|やく|め]なのよ。",
    "translations": {
      "eng": "Sweeping the room is my daughter's job."
    },
    "text": "部屋の掃除は娘の役目なのよ。"
  },
  {
    "id": "9314752",
    "transcription": "[大学|だい|がく]では[何|なに]を[専攻|せん|こう]してたの？",
    "translations": {
      "eng": "What did you specialize in at university?",
      "cmn": "你大學時主修什麼？"
    },
    "text": "大学では何を専攻してたの？"
  },
  {
    "id": "9314758",
    "transcription": "おととい[彼|かれ]に[会|あ]ったんだ。",
    "translations": {
      "eng": "I met him the day before yesterday."
    },
    "text": "おととい彼に会ったんだ。"
  },
  {
    "id": "9314759",
    "transcription": "おとついトムを[見|み]かけたよ。",
    "translations": {
      "eng": "I saw Tom the day before yesterday.",
      "nld": "Ik zag Tom eergisteren.",
      "gos": "k Zaag Tom veurguster.",
      "deu": "Ich habe Tom vorgestern gesehen."
    },
    "text": "おとついトムを見かけたよ。"
  },
  {
    "id": "9314760",
    "transcription": "オルガンが[弾|ひ]けますか？",
    "translations": {
      "eng": "Are you able to play organ?",
      "tha": "เล่นออร์แกนเป็นไหม"
    },
    "text": "オルガンが弾けますか？"
  },
  {
    "id": "9314763",
    "transcription": "[私|わたし]が[遅|おく]れたら、[待|ま]たなくていいからね。",
    "translations": {
      "eng": "In case I am late, you don't have to wait for me."
    },
    "text": "私が遅れたら、待たなくていいからね。"
  },
  {
    "id": "9314765",
    "transcription": "[今日|きょう]は[雨|あめ]になると[思|おも]うよ。",
    "translations": {
      "eng": "I think it's going to rain today."
    },
    "text": "今日は雨になると思うよ。"
  },
  {
    "id": "9314767",
    "transcription": "[今日|きょう]の[新聞|しん|ぶん][読|よ]んだ？",
    "translations": {
      "eng": "Did you read today's paper?"
    },
    "text": "今日の新聞読んだ？"
  },
  {
    "id": "9314771",
    "transcription": "[事前|じ|ぜん]にお[知|し]らせします。",
    "translations": {
      "eng": "I'll let you know beforehand."
    },
    "text": "事前にお知らせします。"
  },
  {
    "id": "9314772",
    "transcription": "あの[辞書|じ|しょ]、[君|きみ]んじゃない？",
    "translations": {
      "eng": "Isn't that your dictionary?"
    },
    "text": "あの辞書、君んじゃない？"
  },
  {
    "id": "9314773",
    "transcription": "[絶対|ぜっ|たい]に[嘘|うそ]はつくな。",
    "translations": {
      "eng": "Never tell a lie."
    },
    "text": "絶対に嘘はつくな。"
  },
  {
    "id": "9314775",
    "transcription": "[台所|だい|どころ]は[後|あと]で[片|かた]づけます。",
    "translations": {
      "eng": "I'll clean up the kitchen later."
    },
    "text": "台所は後で片づけます。"
  },
  {
    "id": "9314776",
    "transcription": "お[釣|つ]りはいいです。",
    "translations": {
      "fin": "Voit pitää vaihtorahat."
    },
    "text": "お釣りはいいです。"
  },
  {
    "id": "9314777",
    "transcription": "お[釣|つ]りは[結構|けっ|こう]です。",
    "translations": {
      "eng": "Keep the change.",
      "spa": "Quédese con el cambio.",
      "tha": "ไม่ต้องทอน"
    },
    "text": "お釣りは結構です。"
  },
  {
    "id": "9314778",
    "transcription": "[彼|かれ]だったらできるよ。",
    "translations": {
      "eng": "He could do it."
    },
    "text": "彼だったらできるよ。"
  },
  {
    "id": "9314793",
    "transcription": "ゴミ[出|だ]しといて。",
    "translations": {
      "eng": "Take the rubbish out.",
      "deu": "Bring den Müll nach draußen."
    },
    "text": "ゴミ出しといて。"
  },
  {
    "id": "9314794",
    "transcription": "[彼|かれ]のことはほっときなさい。",
    "translations": {
      "eng": "Leave him alone."
    },
    "text": "彼のことはほっときなさい。"
  },
  {
    "id": "9314805",
    "transcription": "[洗濯|せん|たく][物干|もの|ほ]しといて。",
    "translations": {
      "deu": "Häng die Wäsche auf!",
      "eng": "Hang out the laundry!"
    },
    "text": "洗濯物干しといて。"
  },
  {
    "id": "9314806",
    "transcription": "[布団|ふ|とん][干|ほ]しといて。",
    "translations": {
      "eng": "Air the futons."
    },
    "text": "布団干しといて。"
  },
  {
    "id": "9314807",
    "transcription": "[迎|むか]えに[来|き]て。",
    "translations": {
      "eng": "Come pick me up.",
      "deu": "Hol mich ab!"
    },
    "text": "迎えに来て。"
  },
  {
    "id": "9314809",
    "transcription": "[態度|たい|ど]に[気|き]を[付|つ]けなさい。",
    "translations": {
      "eng": "Behave yourselves.",
      "deu": "Benehmt euch."
    },
    "text": "態度に気を付けなさい。"
  },
  {
    "id": "9314817",
    "transcription": "[彼女|かの|じょ]は[癌|がん]ではありません。",
    "translations": {
      "eng": "She doesn't have cancer.",
      "epo": "Ŝi ne havas kanceron.",
      "deu": "Sie hat keinen Krebs."
    },
    "text": "彼女は癌ではありません。"
  },
  {
    "id": "9314827",
    "transcription": "[俺|おれ]たちについてこい。",
    "translations": {
      "eng": "Follow us."
    },
    "text": "俺たちについてこい。"
  },
  {
    "id": "9314831",
    "transcription": "[表|おもて]に[出|で]ろ。",
    "translations": {
      "eng": "Step outside."
    },
    "text": "表に出ろ。"
  },
  {
    "id": "9314841",
    "transcription": "[誰|だれ]かに[聞|き]いてみて。",
    "translations": {
      "eng": "Ask anybody."
    },
    "text": "誰かに聞いてみて。"
  },
  {
    "id": "9314845",
    "transcription": "みんなに[伝|つた]えて。",
    "translations": {
      "eng": "Tell everyone.",
      "kor": "모두한테 말해.",
      "fin": "Kerro kaikille.",
      "npi": "सबैलाई भन्नुहोस्।"
    },
    "text": "みんなに伝えて。"
  },
  {
    "id": "9314846",
    "transcription": "[笑|わら]わないで。",
    "translations": {
      "eng": "Don't laugh."
    },
    "text": "笑わないで。"
  },
  {
    "id": "9314848",
    "transcription": "[嘘|うそ]はやめろ。",
    "translations": {
      "eng": "Quit lying.",
      "cmn": "你可別再說瞎話了！"
    },
    "text": "嘘はやめろ。"
  },
  {
    "id": "9314854",
    "transcription": "トムを[止|と]めるんだ。",
    "translations": {
      "eng": "Stop Tom."
    },
    "text": "トムを止めるんだ。"
  },
  {
    "id": "9314855",
    "transcription": "これ[直|なお]して。",
    "translations": {
      "eng": "Fix this."
    },
    "text": "これ直して。"
  },
  {
    "id": "9314860",
    "transcription": "[中|なか]にいなさい。",
    "translations": {
      "eng": "Stay inside."
    },
    "text": "中にいなさい。"
  },
  {
    "id": "9314862",
    "transcription": "[彼|かれ]らに[聞|き]きなさい。",
    "translations": {
      "eng": "Ask them."
    },
    "text": "彼らに聞きなさい。"
  },
  {
    "id": "9314867",
    "transcription": "[何|なに]か[飲|の]んで。",
    "translations": {
      "eng": "Drink something.",
      "ind": "Minumlah sesuatu!"
    },
    "text": "何か飲んで。"
  },
  {
    "id": "9314870",
    "transcription": "しかめっ[面|つら]は、やめろ。",
    "translations": {
      "eng": "Stop scowling."
    },
    "text": "しかめっ面は、やめろ。"
  },
  {
    "id": "9314871",
    "transcription": "[石鹸|せっ|けん]、[取|と]って。",
    "translations": {
      "eng": "Hand me the soap."
    },
    "text": "石鹸、取って。"
  },
  {
    "id": "9314872",
    "transcription": "もっと[金|かね]をくれ。",
    "translations": {
      "eng": "Give me more money.",
      "rus": "Дай мне больше денег.",
      "fra": "Donne-moi plus d'argent.",
      "deu": "Gib mir mehr Geld!",
      "epo": "Donu al mi pli da mono!"
    },
    "text": "もっと金をくれ。"
  },
  {
    "id": "9314874",
    "transcription": "[子守|こ|もり][唄|うた]を[歌|うた]って。",
    "translations": {
      "eng": "Sing me a lullaby.",
      "fin": "Laula minulle tuutulaulu.",
      "pes": "لالایی بخوان."
    },
    "text": "子守唄を歌って。"
  },
  {
    "id": "9314884",
    "transcription": "[君|きみ]が[運転|うん|てん]して。",
    "translations": {
      "eng": "You drive."
    },
    "text": "君が運転して。"
  },
  {
    "id": "9314887",
    "transcription": "[隠|かく]れろ。",
    "translations": {
      "eng": "Hide.",
      "spa": "Escóndete.",
      "por": "Se esconda."
    },
    "text": "隠れろ。"
  },
  {
    "id": "9314891",
    "transcription": "[決|けっ]して[愛|あい]を[諦|あきら]めないで。",
    "translations": {
      "eng": "Never give up on love.",
      "deu": "Gib die Liebe nie auf!"
    },
    "text": "決して愛を諦めないで。"
  },
  {
    "id": "9314896",
    "transcription": "トムはすぐ[近|ちか]くに[住|す]んでるよ。",
    "translations": {
      "deu": "Tom wohnt hier ganz in der Nähe."
    },
    "text": "トムはすぐ近くに住んでるよ。"
  },
  {
    "id": "9314899",
    "transcription": "トムならすぐそこに[住|す]んでるよ。",
    "translations": {
      "eng": "Tom lives close by.",
      "deu": "Tom wohnt gleich da drüben.",
      "fin": "Tomi asuu aivan tuossa lähellä."
    },
    "text": "トムならすぐそこに住んでるよ。"
  }
]