[
  {
    "id": "7429281",
    "transcription": "みんな[待|ま]った。",
    "translations": {
      "eng": "Everybody waited.",
      "tur": "Herkes bekledi.",
      "drt": "Elkenien wachtte.",
      "vie": "Tất cả mọi người đều đã đợi."
    },
    "text": "みんな待った。"
  },
  {
    "id": "7429282",
    "transcription": "トムは[謝|あやま]った。",
    "translations": {
      "eng": "Tom made an apology.",
      "rus": "Том попросил прощения.",
      "deu": "Tom bat um Entschuldigung.",
      "npi": "टमले माफी माग्यो।",
      "tha": "ทอมขอโทษ"
    },
    "text": "トムは謝った。"
  },
  {
    "id": "7429293",
    "transcription": "[彼|かれ]らは[負|ま]けた。",
    "translations": {
      "eng": "They lost.",
      "deu": "Sie haben verloren.",
      "fin": "He hävisivät.",
      "ain": "Toan utar a-maketaro."
    },
    "audio": {
      "id": "753997",
      "author": "lowteq"
    },
    "text": "彼らは負けた。"
  },
  {
    "id": "7429294",
    "transcription": "[彼|かれ]らは[去|さ]った。",
    "translations": {
      "eng": "They left.",
      "fin": "He lähtivät pois.",
      "cmn": "他们离开了。",
      "deu": "Sie sind gegangen.",
      "ain": "oka paye wa isam."
    },
    "audio": {
      "id": "753998",
      "author": "lowteq"
    },
    "text": "彼らは去った。"
  },
  {
    "id": "7429298",
    "transcription": "[彼|かれ]らは[踊|おど]った。",
    "translations": {
      "eng": "They danced.",
      "deu": "Sie tanzten.",
      "fin": "He tanssivat."
    },
    "text": "彼らは踊った。"
  },
  {
    "id": "7429301",
    "transcription": "[彼|かれ]らは[電話|でん|わ]した。",
    "translations": {
      "eng": "They called.",
      "deu": "Sie haben angerufen."
    },
    "text": "彼らは電話した。"
  },
  {
    "id": "7430474",
    "transcription": "[僕|ぼく]らは[話|はな]した。",
    "translations": {
      "eng": "We talked.",
      "fin": "Me puhuimme.",
      "ain": "itak-as."
    },
    "audio": {
      "id": "753999",
      "author": "lowteq"
    },
    "text": "僕らは話した。"
  },
  {
    "id": "7430475",
    "transcription": "[僕|ぼく]らは[待|ま]った。",
    "translations": {
      "eng": "We waited.",
      "fin": "Odotimme.",
      "deu": "Wir warteten.",
      "ain": "coka ci-tere."
    },
    "audio": {
      "id": "754000",
      "author": "lowteq"
    },
    "text": "僕らは待った。"
  },
  {
    "id": "7430486",
    "transcription": "トムは[忘|わす]れた。",
    "translations": {
      "eng": "Tom forgot.",
      "rus": "Том забыл."
    },
    "text": "トムは忘れた。"
  },
  {
    "id": "7430489",
    "transcription": "トムは[終|お]わった。",
    "translations": {
      "eng": "Tom has finished.",
      "rus": "Том закончил.",
      "deu": "Tom wurde fertig."
    },
    "text": "トムは終わった。"
  },
  {
    "id": "7430493",
    "transcription": "トムは[逃|に]げた。",
    "translations": {
      "eng": "Tom fled.",
      "rus": "Том убежал.",
      "fra": "Tom a pris la fuite."
    },
    "text": "トムは逃げた。"
  },
  {
    "id": "7430513",
    "transcription": "トムは[立|た]った。",
    "translations": {
      "eng": "Tom stood.",
      "deu": "Tom stand auf.",
      "fin": "Tomi seisoi.",
      "ain": "Tom as."
    },
    "audio": {
      "id": "754001",
      "author": "lowteq"
    },
    "text": "トムは立った。"
  },
  {
    "id": "7430527",
    "transcription": "[彼|かれ]らは[嘘|うそ]をついた。",
    "translations": {
      "eng": "They lied.",
      "deu": "Sie haben gelogen.",
      "fin": "He valehtelivat.",
      "ain": "oka sunke."
    },
    "audio": {
      "id": "754002",
      "author": "lowteq"
    },
    "text": "彼らは嘘をついた。"
  },
  {
    "id": "7430536",
    "transcription": "[歌|うた]い[始|はじ]めなさい。",
    "translations": {
      "eng": "Start singing.",
      "rus": "Запевайте.",
      "vie": "Bắt đầu hát đi.",
      "slk": "Začnite spievať."
    },
    "text": "歌い始めなさい。"
  },
  {
    "id": "7430537",
    "transcription": "[走|はし]り[始|はじ]めなさい。",
    "translations": {
      "eng": "Start running.",
      "rus": "Начинайте бежать."
    },
    "text": "走り始めなさい。"
  },
  {
    "id": "7432598",
    "transcription": "[10月|じゅう||がつ]にまた[会|あ]おう。",
    "translations": {
      "eng": "Let's meet again in October.",
      "deu": "Treffen wir uns im Oktober wieder!",
      "rus": "Давай в октябре снова встретимся.",
      "npi": "अक्टोबरमा भेटौँला।",
      "ind": "Mari bertemu lagi pada bulan Oktober."
    },
    "text": "10月にまた会おう。"
  },
  {
    "id": "7432603",
    "transcription": "[謝|あやま]った。",
    "translations": {
      "eng": "I apologized.",
      "fin": "Minä pyysin anteeksi.",
      "pol": "Przeprosiłem.",
      "vie": "Tôi đã nói lời xin lỗi."
    },
    "text": "謝った。"
  },
  {
    "id": "7432607",
    "transcription": "トムは[幸|しあわ]せそうだった。",
    "translations": {
      "eng": "Tom seemed happy.",
      "rus": "Том выглядел счастливым."
    },
    "text": "トムは幸せそうだった。"
  },
  {
    "id": "7432609",
    "transcription": "トムは[貧|まず]しい。",
    "translations": {
      "eng": "Tom is poor.",
      "fra": "Tom est pauvre.",
      "deu": "Tom ist arm.",
      "rus": "Том бедный.",
      "swe": "Tom är fattig.",
      "ukr": "Том бідний.",
      "nld": "Tom is arm.",
      "pol": "Tom jest biedny."
    },
    "text": "トムは貧しい。"
  },
  {
    "id": "7432612",
    "transcription": "トムは[緊張|きん|ちょう]した。",
    "translations": {
      "eng": "Tom became nervous.",
      "deu": "Tom wurde nervös."
    },
    "text": "トムは緊張した。"
  },
  {
    "id": "7453562",
    "transcription": "トムは[卒業|そつ|ぎょう]した。",
    "translations": {
      "eng": "Tom graduated.",
      "deu": "Tom machte seinen Abschluss.",
      "cmn": "湯姆畢業了。"
    },
    "text": "トムは卒業した。"
  },
  {
    "id": "7459024",
    "transcription": "[犬|いぬ]に[餌|えさ]をやるのを[忘|わす]れた。",
    "translations": {
      "eng": "I forgot to feed my dog.",
      "deu": "Ich habe vergessen, den Hund zu füttern.",
      "rus": "Я забыл покормить свою собаку."
    },
    "text": "犬に餌をやるのを忘れた。"
  },
  {
    "id": "7459033",
    "transcription": "トムはメアリーの[車|くるま]を[洗|あら]った。",
    "translations": {
      "eng": "Tom washed Mary's car.",
      "deu": "Tom wusch Marias Wagen.",
      "rus": "Том помыл машину Мэри."
    },
    "text": "トムはメアリーの車を洗った。"
  },
  {
    "id": "7459040",
    "transcription": "あなたの[馬|うま]はどこ？",
    "translations": {
      "eng": "Where are your horses?",
      "deu": "Wo ist dein Pferd?",
      "vie": "Ngựa của bạn ở đâu rồi?"
    },
    "text": "あなたの馬はどこ？"
  },
  {
    "id": "7461947",
    "transcription": "[今日|きょう]は[何|なに]をしていましたか？",
    "translations": {
      "rus": "Что ты сегодня делал?",
      "eng": "What were you doing today?"
    },
    "text": "今日は何をしていましたか？"
  },
  {
    "id": "7461949",
    "transcription": "トムは[今日|きょう][何|なに]をしていましたか？",
    "translations": {
      "eng": "What was Tom doing today?",
      "rus": "Что Том делал сегодня?"
    },
    "text": "トムは今日何をしていましたか？"
  },
  {
    "id": "7461951",
    "transcription": "[何|なん]でやってみないの？",
    "translations": {
      "eng": "Why don't you just try it?"
    },
    "text": "何でやってみないの？"
  },
  {
    "id": "7461955",
    "transcription": "[何|なん]で[馬|うま]が[欲|ほ]しいの？",
    "translations": {
      "eng": "Why do you want a horse?",
      "cmn": "你為什麼想要一匹馬？",
      "deu": "Warum willst du ein Pferd?"
    },
    "text": "何で馬が欲しいの？"
  },
  {
    "id": "7461962",
    "transcription": "[僕|ぼく]は[最初|さい|しょ]すごい[緊張|きん|ちょう]した。",
    "translations": {
      "eng": "I was very nervous at first.",
      "rus": "Поначалу я сильно нервничал.",
      "fin": "Mä olin aluksi hirveän hermoissani."
    },
    "text": "僕は最初すごい緊張した。"
  },
  {
    "id": "7461964",
    "transcription": "トムにより[細|こま]かいことを[尋|たず]ねた。",
    "translations": {
      "eng": "I asked Tom for more details."
    },
    "text": "トムにより細かいことを尋ねた。"
  },
  {
    "id": "7461979",
    "transcription": "[昔|むかし]ジョークを[言|い]うのが[大好|だい|す]きだった。",
    "translations": {
      "eng": "I used to love telling jokes.",
      "rus": "Раньше я любил рассказывать шутки."
    },
    "text": "昔ジョークを言うのが大好きだった。"
  },
  {
    "id": "7461983",
    "transcription": "トムは[有名|ゆう|めい]なアイドルだ。",
    "translations": {
      "eng": "Tom is a famous pop star."
    },
    "text": "トムは有名なアイドルだ。"
  },
  {
    "id": "7461995",
    "transcription": "[仕事|し|ごと]の[後|あと][集|あつ]まりたい？",
    "translations": {
      "eng": "Do you want to get together after work?",
      "rus": "Хотите собраться после работы?",
      "fra": "On se rejoint après le boulot ?"
    },
    "text": "仕事の後集まりたい？"
  },
  {
    "id": "7461998",
    "transcription": "トムは[飲|の]んでる？",
    "translations": {
      "eng": "Is Tom drinking?"
    },
    "text": "トムは飲んでる？"
  },
  {
    "id": "7462000",
    "transcription": "この[椅子|い|す][大好|だい|す]き。",
    "translations": {
      "deu": "Ich liebe diesen Stuhl.",
      "eng": "I love this chair.",
      "rus": "Я люблю этот стул."
    },
    "text": "この椅子大好き。"
  },
  {
    "id": "7462002",
    "transcription": "トムは[料理|りょう|り]する。",
    "translations": {
      "eng": "Tom cooks.",
      "rus": "Том готовит.",
      "deu": "Tom kocht.",
      "slk": "Tom varí.",
      "hsb": "Tom wari.",
      "fin": "Tomi kokkaa.",
      "ain": "Tom suke."
    },
    "audio": {
      "id": "754003",
      "author": "lowteq"
    },
    "text": "トムは料理する。"
  },
  {
    "id": "7462011",
    "transcription": "トムは[飲|の]んだ。",
    "translations": {
      "eng": "Tom drank.",
      "fin": "Tom joi.",
      "spa": "Tom bebió.",
      "deu": "Tom hat es getrunken.",
      "ain": "Tom ku."
    },
    "audio": {
      "id": "754004",
      "author": "lowteq"
    },
    "text": "トムは飲んだ。"
  },
  {
    "id": "7462015",
    "transcription": "トムと[一緒|いっ|しょ]に[昼|ひる]ご[飯|はん]を[食|た]べた。",
    "translations": {
      "eng": "I ate lunch with Tom.",
      "rus": "Я обедал с Томом.",
      "deu": "Ich habe mit Tom zu Mittag gegessen."
    },
    "text": "トムと一緒に昼ご飯を食べた。"
  },
  {
    "id": "7462017",
    "transcription": "トムはやあと[言|い]った。",
    "translations": {
      "eng": "Tom said hi.",
      "cmn": "汤姆打了个招呼。"
    },
    "text": "トムはやあと言った。"
  },
  {
    "id": "7462019",
    "transcription": "[10月|じゅうがつ]にトムと[会|あ]った。",
    "translations": {
      "eng": "I met Tom in October.",
      "rus": "Я встретился с Томом в октябре."
    },
    "text": "10月にトムと会った。"
  },
  {
    "id": "7463930",
    "transcription": "トムはメアリーの[話|はなし]が[好|す]きだった。",
    "translations": {
      "eng": "Tom liked Mary's story.",
      "deu": "Tom gefiel Marias Geschichte."
    },
    "text": "トムはメアリーの話が好きだった。"
  },
  {
    "id": "7463934",
    "transcription": "トムと[僕|ぼく]はたくさん[喧嘩|けん|か]した。",
    "translations": {
      "deu": "Tom und ich haben uns viel gestritten.",
      "vie": "Tom và tôi đã rất hay cãi cọ với nhau."
    },
    "text": "トムと僕はたくさん喧嘩した。"
  },
  {
    "id": "7463935",
    "transcription": "[僕|ぼく]は[一|いち][度|ど]も[勝|か]ったことがない。",
    "translations": {
      "eng": "I've never won anything."
    },
    "text": "僕は一度も勝ったことがない。"
  },
  {
    "id": "7463936",
    "transcription": "サーフィンはすごく[楽|たの]しい。",
    "translations": {
      "eng": "Surfing is a lot of fun.",
      "vie": "Lướt sóng rất vui."
    },
    "text": "サーフィンはすごく楽しい。"
  },
  {
    "id": "7464011",
    "transcription": "メアリーは[彼女|かの|じょ]のお[母|かあ]さんに[声|こえ]がそっくりだ。",
    "translations": {
      "eng": "Mary sounds just like her mother.",
      "deu": "Maria klingt wie ihre Mutter.",
      "rus": "Голос Мэри совсем как у её мамы."
    },
    "text": "メアリーは彼女のお母さんに声がそっくりだ。"
  },
  {
    "id": "7464015",
    "transcription": "[僕|ぼく]は[議論|ぎ|ろん]する[事|こと]が[好|す]きではない。",
    "translations": {
      "eng": "I don't like arguing.",
      "deu": "Ich streite nicht gern.",
      "vie": "Tôi không thích tranh luận.",
      "nld": "Ik hou niet van ruzie maken."
    },
    "text": "僕は議論する事が好きではない。"
  },
  {
    "id": "7464039",
    "transcription": "トムと[僕|ぼく]はたくさん[闘|たたか]った。",
    "translations": {
      "eng": "Tom and I fought a lot."
    },
    "text": "トムと僕はたくさん闘った。"
  },
  {
    "id": "7464320",
    "transcription": "[君|きみ]の[娘|むすめ]たちはどこ？",
    "translations": {
      "eng": "Where are your daughters?",
      "spa": "¿Dónde están tus hijas?"
    },
    "text": "君の娘たちはどこ？"
  },
  {
    "id": "7465217",
    "transcription": "トムは[真夜中|ま|よ|なか]についた。",
    "translations": {
      "eng": "Tom arrived at midnight.",
      "fra": "Tom arriva à minuit.",
      "deu": "Tom ist um Mitternacht angekommen."
    },
    "text": "トムは真夜中についた。"
  },
  {
    "id": "7465221",
    "transcription": "トムはスポーツが[好|す]きじゃない。",
    "translations": {
      "eng": "Tom doesn't like sports.",
      "rus": "Том не любит спорт."
    },
    "text": "トムはスポーツが好きじゃない。"
  }
]